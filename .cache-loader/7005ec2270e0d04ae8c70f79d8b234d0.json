E,QAAQ,CAACQ,WAAT,EAA7B,YADF,CADF,CALF,eAUE,oBAAC,aAAD,qBACE,oBAAC,UAAD;AAAY,MAAA,KAAK,EAAC;AAAlB,oBACE,oBAAC,cAAD;AACE,MAAA,MAAM,EAAC,aADT;AAEE,MAAA,KAAK,EAAC,SAFR;AAGE,MAAA,IAAI,EAAC,GAHP;AAIE,MAAA,OAAO,EAAEN,KAJX;AAKE,MAAA,UAAU,EAAEJ,IALd;AAME,MAAA,QAAQ,EAAE,kBAACK,EAAD,EAAQ;AAChB,YAAMM,OAAO,GAAGP,KAAK,CAClBQ,IADa,CACR,UAACZ,IAAD;AAAA,iBAAUA,IAAI,CAACK,EAAL,KAAYA,EAAtB;AAAA,SADQ,EAEbC,KAFa,CAEPI,WAFO,EAAhB;AAGAT,QAAAA,OAAO,CAACI,EAAD,CAAP;AACAF,QAAAA,WAAW,CAACQ,OAAD,CAAX;AACD;AAZH,MADF,CADF,CAVF,CADF;AA+BD;;AACD,sBACE,8CACE,oBAAC,SAAD;AAAW,IAAA,OAAO,EAAEH;AAApB,kCADF,EAEGC,MAFH,CADF;AAMD,CApED","sourcesContent":["import React, { useState } from 'react';\n\nimport {\n  EuiFlyout,\n  EuiFlyoutHeader,\n  EuiFlyoutBody,\n  EuiButton,\n  EuiTitle,\n  EuiFormRow,\n  EuiButtonGroup,\n} from '../../../../src/components';\n\nexport default () => {\n  const [isFlyoutVisible, setIsFlyoutVisible] = useState(false);\n  const [size, setSize] = useState('l');\n  const [sizeName, setSizeName] = useState('large');\n\n  const sizes = [\n    {\n      id: 's',\n      label: 'Small',\n    },\n    {\n      id: 'm',\n      label: 'Medium',\n    },\n    {\n      id: 'l',\n      label: 'Large',\n    },\n    {\n      id: '400px',\n      label: 'Fixed (400)',\n    },\n  ];\n\n  const closeFlyout = () => setIsFlyoutVisible(false);\n\n  const showFlyout = () => setIsFlyoutVisible(true);\n\n  let flyout;\n  if (isFlyoutVisible) {\n    flyout = (\n      <EuiFlyout\n        ownFocus\n        onClose={closeFlyout}\n        size={size}\n        aria-labelledby=\"flyoutLargeTitle\">\n        <EuiFlyoutHeader hasBorder>\n          <EuiTitle size=\"m\">\n            <h2 id=\"flyoutLargeTitle\">A {sizeName.toLowerCase()} flyout</h2>\n          </EuiTitle>\n        </EuiFlyoutHeader>\n        <EuiFlyoutBody>\n          <EuiFormRow label=\"Change the paddingSize\">\n            <EuiButtonGroup\n              legend=\"Flyout size\"\n              color=\"primary\"\n              size=\"s\"\n              options={sizes}\n              idSelected={size}\n              onChange={(id) => {\n                const newName = sizes\n                  .find((size) => size.id === id)\n                  .label.toLowerCase();\n                setSize(id);\n                setSizeName(newName);\n              }}\n            />\n          </EuiFormRow>\n        </EuiFlyoutBody>\n      </EuiFlyout>\n    );\n  }\n  return (\n    <div>\n      <EuiButton onClick={showFlyout}>Show flyout to test widths</EuiButton>\n      {flyout}\n    </div>\n  );\n};\n"]}]}
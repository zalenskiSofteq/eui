D;AAAA,aAAOD,SAAQ,CAACC,CAAD,CAAf;AAAA,KAHZ;AAIE,IAAA,UAAU,MAJZ;AAKE,IAAA,QAAQ;AALV,IApCF,eA4CE,oBAAC,SAAD;AAAW,IAAA,IAAI,EAAC;AAAhB,IA5CF,eA8CE,oBAAC,SAAD;AACE,IAAA,SAAS,EAAE,KADb;AAEE,IAAA,KAAK,EAAC,kDAFR;AAGE,IAAA,OAAO,EAAEH,OAHX;AAIE,IAAA,QAAQ,EAAE,kBAACG,CAAD;AAAA,aAAOD,SAAQ,CAACC,CAAD,CAAf;AAAA,KAJZ;AAKE,IAAA,UAAU;AALZ,IA9CF,CADF;AAwDD,CA/DD","sourcesContent":["import React, { useState, Fragment } from 'react';\n\nimport { EuiSwitch, EuiSpacer } from '../../../../src/components';\n\nexport default () => {\n  const [checked, setChecked] = useState(false);\n\n  const onChange = (e) => {\n    setChecked(e.target.checked);\n  };\n\n  return (\n    <Fragment>\n      <EuiSwitch\n        label=\"I am a switch\"\n        checked={checked}\n        onChange={(e) => onChange(e)}\n      />\n\n      <EuiSpacer size=\"m\" />\n\n      <EuiSwitch\n        label=\"I am a disabled switch\"\n        checked={checked}\n        onChange={(e) => onChange(e)}\n        disabled\n      />\n\n      <EuiSpacer size=\"m\" />\n\n      <EuiSwitch\n        showLabel={false}\n        label=\"I am a switch without a visible label\"\n        checked={checked}\n        onChange={(e) => onChange(e)}\n      />\n\n      <EuiSpacer size=\"m\" />\n\n      <EuiSwitch\n        label=\"I am a compressed switch\"\n        checked={checked}\n        onChange={(e) => onChange(e)}\n        compressed\n      />\n\n      <EuiSpacer size=\"m\" />\n\n      <EuiSwitch\n        label=\"I am a compressed, disabled switch\"\n        checked={checked}\n        onChange={(e) => onChange(e)}\n        compressed\n        disabled\n      />\n\n      <EuiSpacer size=\"m\" />\n\n      <EuiSwitch\n        showLabel={false}\n        label=\"I am a compressed switch without a visible label\"\n        checked={checked}\n        onChange={(e) => onChange(e)}\n        compressed\n      />\n    </Fragment>\n  );\n};\n"]}]}
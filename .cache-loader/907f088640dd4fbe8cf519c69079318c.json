AuB,EAAAA,YAAY,CAACC,aAAD,CAAZ;AAEA,MAAMA,aAAa,GAAGC,UAAU,CAAC,YAAM;AACrC;AACAjB,IAAAA,UAAU,CAAC,KAAD,CAAV;AACD,GAH+B,EAG7B,IAH6B,CAAhC;;AAKA,MAAMkB,MAAM,GAAG,SAATA,MAAS,CAACC,KAAD,EAAW;AACxB,wBACE,0CACGT,IAAI,CAACU,GAAL,CAAS,UAACC,KAAD,EAAW;AACnB,UAAIC,QAAJ;;AACA,UAAI7B,IAAJ,EAAU;AACR6B,QAAAA,QAAQ,gBAAG,oBAAC,OAAD;AAAS,UAAA,IAAI,EAAC,KAAd;AAAoB,UAAA,KAAK,EAAED,KAAK,CAACR;AAAjC,UAAX;AACD,OAFD,MAEO,IAAIhB,OAAJ,EAAa;AAClByB,QAAAA,QAAQ,gBAAG,oBAAC,SAAD;AAAW,UAAA,IAAI,EAAC,GAAhB;AAAoB,UAAA,IAAI,EAAED,KAAK,CAACV;AAAhC,UAAX;AACD;;AAED,0BACE,oBAAC,cAAD;AACE,QAAA,GAAG,EAAEU,KAAK,CAAChB,EADb;AAEE,QAAA,EAAE,YAAKgB,KAAK,CAAChB,EAAX,cAAiBc,KAAjB,CAFJ;AAGE,QAAA,QAAQ,EAAEE,KAAK,CAACT,QAHlB;AAIE,QAAA,IAAI,EAAEU,QAJR;AAKE,QAAA,UAAU,EAAEpB,gBAAgB,KAAKmB,KAAK,CAAChB,EALzC;AAME,QAAA,UAAU,EAAEV,QAAQ,IAAI0B,KAAK,CAAChB,EAAN,KAAa,QANvC;AAOE,QAAA,SAAS,EAAEN,OAPb;AAQE,QAAA,OAAO,EACLsB,KAAK,CAACP,OAAN,GAAgB;AAAA,iBAAMO,KAAK,CAACP,OAAN,CAAcO,KAAK,CAAChB,EAApB,CAAN;AAAA,SAAhB,GAAgDJ;AATpD,SAWGoB,KAAK,CAACV,KAXT,CADF;AAeD,KAvBA,CADH,CADF;AA4BD,GA7BD;;AA+BA,sBACE,8CACE,oBAAC,QAAD;AAAU,IAAA,IAAI,EAAC;AAAf,kBACE,2CADF,CADF,eAIE,oBAAC,aAAD;AAAe,IAAA,KAAK,EAAE;AAAEY,MAAAA,QAAQ,EAAE;AAAZ;AAAtB,KACGL,MAAM,CAAC,UAAD,CADT,CAJF,eAQE,oBAAC,SAAD,OARF,eAUE,oBAAC,QAAD;AAAU,IAAA,IAAI,EAAC;AAAf,kBACE,8DADF,CAVF,eAaE,oBAAC,aAAD;AAAe,IAAA,MAAM,EAAC,YAAtB;AAAmC,IAAA,UAAU,EAAC;AAA9C,KACGA,MAAM,CAAC,YAAD,CADT,CAbF,CADF;AAmBD,CAxJD","sourcesContent":["import React, { useState } from 'react';\n\nimport {\n  EuiFacetButton,\n  EuiFacetGroup,\n  EuiIcon,\n  EuiAvatar,\n  EuiTitle,\n  EuiSpacer,\n} from '../../../../src/components';\n\nimport { euiPaletteColorBlind } from '../../../../src/services';\n\nexport default () => {\n  const [icon, setIcon] = useState(false);\n  const [disabled, setDisabled] = useState(false);\n  const [avatars, setAvatars] = useState(false);\n  const [loading, setLoading] = useState(false);\n  const [selectedOptionId, setSelectedOptionId] = useState(undefined);\n\n  const facet0Clicked = (id) => {\n    setIcon(false);\n    setDisabled(false);\n    setAvatars(false);\n    setLoading(false);\n    setSelectedOptionId((selectedOptionId) =>\n      selectedOptionId === id ? undefined : id\n    );\n  };\n\n  const facet1Clicked = (id) => {\n    setIcon(true);\n    setDisabled(false);\n    setAvatars(false);\n    setLoading(false);\n    setSelectedOptionId((selectedOptionId) =>\n      selectedOptionId === id ? undefined : id\n    );\n  };\n\n  const facet2Clicked = (id) => {\n    setDisabled((disabled) => !disabled);\n    setSelectedOptionId((selectedOptionId) =>\n      selectedOptionId === id ? undefined : id\n    );\n  };\n\n  const facet3Clicked = (id) => {\n    setIcon(false);\n    setDisabled(false);\n    setAvatars(true);\n    setLoading(false);\n    setSelectedOptionId((selectedOptionId) =>\n      selectedOptionId === id ? undefined : id\n    );\n  };\n\n  const facet4Clicked = (id) => {\n    setLoading(true);\n    setSelectedOptionId((selectedOptionId) =>\n      selectedOptionId === id ? undefined : id\n    );\n  };\n\n  const list = [\n    {\n      id: 'facet0',\n      label: 'Simple, no icon',\n      quantity: 6,\n      iconColor: euiPaletteColorBlind()[0],\n      onClick: facet0Clicked,\n    },\n    {\n      id: 'facet1',\n      label: 'Label or color indicator',\n      quantity: 60,\n      iconColor: euiPaletteColorBlind()[1],\n      onClick: facet1Clicked,\n    },\n    {\n      id: 'facet2',\n      label: 'Disable all others',\n      quantity: 600,\n      iconColor: euiPaletteColorBlind()[2],\n      onClick: facet2Clicked,\n    },\n    {\n      id: 'facet3',\n      label: 'Avatars instead of icons',\n      quantity: 60,\n      iconColor: euiPaletteColorBlind()[3],\n      onClick: facet3Clicked,\n    },\n    {\n      id: 'facet4',\n      label: 'Show all as loading',\n      quantity: 6,\n      iconColor: euiPaletteColorBlind()[4],\n      onClick: facet4Clicked,\n    },\n    {\n      id: 'facet5',\n      label: 'Just here to show truncation of really long labels',\n      quantity: 0,\n      iconColor: euiPaletteColorBlind()[5],\n    },\n  ];\n\n  clearTimeout(searchTimeout);\n\n  const searchTimeout = setTimeout(() => {\n    // Simulate a remotely-executed search.\n    setLoading(false);\n  }, 1200);\n\n  const facets = (align) => {\n    return (\n      <>\n        {list.map((facet) => {\n          let iconNode;\n          if (icon) {\n            iconNode = <EuiIcon type=\"dot\" color={facet.iconColor} />;\n          } else if (avatars) {\n            iconNode = <EuiAvatar size=\"s\" name={facet.label} />;\n          }\n\n          return (\n            <EuiFacetButton\n              key={facet.id}\n              id={`${facet.id}_${align}`}\n              quantity={facet.quantity}\n              icon={iconNode}\n              isSelected={selectedOptionId === facet.id}\n              isDisabled={disabled && facet.id !== 'facet2'}\n              isLoading={loading}\n              onClick={\n                facet.onClick ? () => facet.onClick(facet.id) : undefined\n              }>\n              {facet.label}\n            </EuiFacetButton>\n          );\n        })}\n      </>\n    );\n  };\n\n  return (\n    <div>\n      <EuiTitle size=\"s\">\n        <h3>Vertical</h3>\n      </EuiTitle>\n      <EuiFacetGroup style={{ maxWidth: 200 }}>\n        {facets('Vertical')}\n      </EuiFacetGroup>\n\n      <EuiSpacer />\n\n      <EuiTitle size=\"s\">\n        <h3>Horizontal and large gutter</h3>\n      </EuiTitle>\n      <EuiFacetGroup layout=\"horizontal\" gutterSize=\"l\">\n        {facets('Horizontal')}\n      </EuiFacetGroup>\n    </div>\n  );\n};\n"]}]}
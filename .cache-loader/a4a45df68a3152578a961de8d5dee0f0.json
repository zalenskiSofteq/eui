C,0BAA0B,mCAC3BL,uBAD2B,uBAG3BI,QAH2B,EAGhB,CAACJ,uBAAuB,CAACI,QAAD,CAHR,EAAhC;;AAOAH,IAAAA,0BAA0B,CAACI,0BAAD,CAA1B;AACD,GATD;;AAWA,sBACE,oBAAC,OAAD;AAAS,IAAA,SAAS,EAAC;AAAnB,kBACE,oBAAC,UAAD;AAAY,IAAA,KAAK,EAAC,YAAlB;AAA+B,IAAA,QAAQ,EAAC;AAAxC,kBACE,oBAAC,YAAD;AAAc,IAAA,IAAI,EAAC;AAAnB,IADF,CADF,eAKE,oBAAC,UAAD;AACE,IAAA,KAAK,EAAC,oCADR;AAEE,IAAA,WAAW,eACT,oBAAC,OAAD;AAAS,MAAA,IAAI,EAAC;AAAd,oBACE,oBAAC,OAAD,4BADF;AAHJ,kBAOE,oBAAC,SAAD;AACE,IAAA,qBAAqB,MADvB;AAEE,IAAA,OAAO,EAAE,CACP;AAAEC,MAAAA,KAAK,EAAE,YAAT;AAAuBC,MAAAA,IAAI,EAAE;AAA7B,KADO,EAEP;AAAED,MAAAA,KAAK,EAAE,YAAT;AAAuBC,MAAAA,IAAI,EAAE;AAA7B,KAFO,EAGP;AAAED,MAAAA,KAAK,EAAE,cAAT;AAAyBC,MAAAA,IAAI,EAAE;AAA/B,KAHO;AAFX,IAPF,CALF,eAsBE,oBAAC,UAAD;AAAY,IAAA,KAAK,EAAC;AAAlB,kBACE,oBAAC,aAAD,OADF,CAtBF,eA0BE,oBAAC,UAAD;AAAY,IAAA,KAAK,EAAC;AAAlB,kBACE,oBAAC,QAAD;AAAU,IAAA,GAAG,EAAE,CAAf;AAAkB,IAAA,GAAG,EAAE,GAAvB;AAA4B,IAAA,IAAI,EAAC,OAAjC;AAAyC,IAAA,EAAE,EAAC;AAA5C,IADF,CA1BF,eA8BE,oBAAC,UAAD;AACE,IAAA,KAAK,EAAC,4EADR;AAEE,IAAA,aAAa,EAAE;AAFjB,kBAGE,oBAAC,SAAD;AACE,IAAA,IAAI,EAAC,QADP;AAEE,IAAA,KAAK,EAAC,oBAFR;AAGE,IAAA,OAAO,EAAEb,eAHX;AAIE,IAAA,QAAQ,EAAEQ;AAJZ,IAHF,CA9BF,eAyCE,oBAAC,SAAD,OAzCF,eA2CE,oBAAC,gBAAD;AACE,IAAA,OAAO,EAAEN,UADX;AAEE,IAAA,eAAe,EAAEI,uBAFnB;AAGE,IAAA,QAAQ,EAAEG,gBAHZ;AAIE,IAAA,MAAM,EAAE;AACNK,MAAAA,QAAQ,EACN;AAFI;AAJV,IA3CF,eAqDE,oBAAC,SAAD,OArDF,eAuDE,oBAAC,SAAD;AAAW,IAAA,IAAI,EAAC,QAAhB;AAAyB,IAAA,IAAI;AAA7B,iBAvDF,CADF;AA6DD,CAjGD","sourcesContent":["import React, { useState, useRef } from 'react';\n\nimport {\n  EuiButton,\n  EuiCheckboxGroup,\n  EuiFieldText,\n  EuiForm,\n  EuiFormRow,\n  EuiFilePicker,\n  EuiLink,\n  EuiRange,\n  EuiSelect,\n  EuiSpacer,\n  EuiSwitch,\n  EuiText,\n} from '../../../../src/components';\n\nimport { htmlIdGenerator } from '../../../../src/services';\n\nexport default () => {\n  const idPrefix = useRef(htmlIdGenerator()());\n  const [isSwitchChecked, setIsSwitchChecked] = useState(false);\n  const checkboxes = [\n    {\n      id: `${idPrefix.current}0`,\n      label: 'Option one',\n    },\n    {\n      id: `${idPrefix.current}1`,\n      label: 'Option two is checked by default',\n    },\n    {\n      id: `${idPrefix.current}2`,\n      label: 'Option three',\n    },\n  ];\n  const [checkboxIdToSelectedMap, setCheckboxIdToSelectedMap] = useState({\n    [`${idPrefix.current}1`]: true,\n  });\n\n  const onSwitchChange = () => {\n    setIsSwitchChecked(!isSwitchChecked);\n  };\n\n  const onCheckboxChange = (optionId) => {\n    const newCheckboxIdToSelectedMap = {\n      ...checkboxIdToSelectedMap,\n      ...{\n        [optionId]: !checkboxIdToSelectedMap[optionId],\n      },\n    };\n\n    setCheckboxIdToSelectedMap(newCheckboxIdToSelectedMap);\n  };\n\n  return (\n    <EuiForm component=\"form\">\n      <EuiFormRow label=\"Text field\" helpText=\"I am some friendly help text.\">\n        <EuiFieldText name=\"first\" />\n      </EuiFormRow>\n\n      <EuiFormRow\n        label=\"Select (with no initial selection)\"\n        labelAppend={\n          <EuiText size=\"xs\">\n            <EuiLink>Link to some help</EuiLink>\n          </EuiText>\n        }>\n        <EuiSelect\n          hasNoInitialSelection\n          options={[\n            { value: 'option_one', text: 'Option one' },\n            { value: 'option_two', text: 'Option two' },\n            { value: 'option_three', text: 'Option three' },\n          ]}\n        />\n      </EuiFormRow>\n\n      <EuiFormRow label=\"File picker\">\n        <EuiFilePicker />\n      </EuiFormRow>\n\n      <EuiFormRow label=\"Range\">\n        <EuiRange min={0} max={100} name=\"range\" id=\"range\" />\n      </EuiFormRow>\n\n      <EuiFormRow\n        label=\"Use a switch instead of a single checkbox and set 'hasChildLabel' to false\"\n        hasChildLabel={false}>\n        <EuiSwitch\n          name=\"switch\"\n          label=\"Should we do this?\"\n          checked={isSwitchChecked}\n          onChange={onSwitchChange}\n        />\n      </EuiFormRow>\n\n      <EuiSpacer />\n\n      <EuiCheckboxGroup\n        options={checkboxes}\n        idToSelectedMap={checkboxIdToSelectedMap}\n        onChange={onCheckboxChange}\n        legend={{\n          children:\n            'Checkbox groups should use the `legend` prop instead of form row',\n        }}\n      />\n\n      <EuiSpacer />\n\n      <EuiButton type=\"submit\" fill>\n        Save form\n      </EuiButton>\n    </EuiForm>\n  );\n};\n"]}]}
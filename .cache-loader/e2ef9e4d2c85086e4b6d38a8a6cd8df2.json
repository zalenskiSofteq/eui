P;AAEE,uCACEH,gBAAgB,GAAG,mBAAH,GAAyB,YAD3C,CAFF;AAKE,IAAA,YAAY,EAAEA,gBAAgB,IAAIC;AALpC,kBAME,oBAAC,OAAD;AAAS,IAAA,IAAI,EAAC;AAAd,IANF,CADF;AAWA,sBACE,0CACGK,UADH,EAEGC,WAFH,CADF;AAMD,CAjD6B,CAAhC;AAmDAT,aAAa,CAACU,WAAd,GAA4B,eAA5B;AAEA,gBAAe,YAAM;AAAA,kBAC6BnB,QAAQ,CAAC,KAAD,CADrC;AAAA;AAAA,MACZW,gBADY;AAAA,MACMS,mBADN;;AAEnB,MAAMC,gBAAgB,GAAGtB,MAAM,EAA/B;;AAFmB,mBAGmCC,QAAQ,CAAC,CAAD,CAH3C;AAAA;AAAA,MAGZY,mBAHY;AAAA,MAGSU,sBAHT;;AAKnB,MAAMC,MAAM,GAAG,SAATA,MAAS,GAAM;AAAA;;AACnB,QAAI,CAACZ,gBAAL,EAAuB;AACrBW,MAAAA,sBAAsB,CAAC,CAAD,CAAtB;AACAF,MAAAA,mBAAmB,CAAC,IAAD,CAAnB;AACD,KAHD,MAGO;AACLE,MAAAA,sBAAsB,CAACV,mBAAmB,GAAG,CAAvB,CAAtB;AACD;;AAED,6BAAAS,gBAAgB,CAACL,OAAjB,gFAA0BD,UAA1B;AACD,GATD;;AAWA,sBACE,uDACE,oBAAC,YAAD;AAAc,IAAA,UAAU,EAAE,KAA1B;AAAiC,IAAA,UAAU,EAAC,QAA5C;AAAqD,IAAA,UAAU,EAAC;AAAhE,kBACE,oBAAC,WAAD;AAAa,IAAA,IAAI,EAAE;AAAnB,kBACE,oBAAC,SAAD;AAAW,IAAA,IAAI,EAAC,GAAhB;AAAoB,IAAA,OAAO,EAAEQ;AAA7B,wBADF,CADF,eAOE,oBAAC,WAAD;AAAa,IAAA,IAAI,EAAE;AAAnB,kBACE,oBAAC,SAAD;AACE,IAAA,IAAI,EAAC,GADP;AAEE,IAAA,KAAK,EAAC,SAFR;AAGE,IAAA,OAAO,EAAE,mBAAM;AACbH,MAAAA,mBAAmB,CAAC,KAAD,CAAnB;AACAE,MAAAA,sBAAsB,CAAC,CAAD,CAAtB;AACD;AANH,aADF,CAPF,CADF,eAqBE,oBAAC,SAAD,OArBF,eAuBE,oBAAC,SAAD;AACE,IAAA,QAAQ,EAAE,CACR;AACEE,MAAAA,KAAK,EAAE,cAAC,oBAAC,aAAD,kBAAD,CADT;AAEEC,MAAAA,OAAO,EAAE;AAFX,KADQ,EAKR;AACED,MAAAA,KAAK,EAAE,cACL,oBAAC,aAAD;AACE,QAAA,GAAG,EAAEH,gBADP;AAEE,QAAA,gBAAgB,EAAEV,gBAFpB;AAGE,QAAA,mBAAmB,EAAES,mBAHvB;AAIE,QAAA,mBAAmB,EAAER;AAJvB,QADK,CADT;AASEa,MAAAA,OAAO,EAAE;AATX,KALQ;AADZ,IAvBF,CADF;AA6CD,CA7DD","sourcesContent":["import React, {\n  forwardRef,\n  useCallback,\n  useImperativeHandle,\n  useRef,\n  useState,\n} from 'react';\n\nimport {\n  EuiButton,\n  EuiFlexGroup,\n  EuiFlexItem,\n  EuiHeader,\n  EuiHeaderLogo,\n  EuiHeaderSectionItemButton,\n  EuiIcon,\n  EuiSpacer,\n} from '../../../../src/components';\n\nconst HeaderUpdates = forwardRef(\n  ({ showNotification, notificationsNumber }, ref) => {\n    const bellRef = useRef();\n    const cheerRef = useRef();\n\n    // wrapping the `euiAnimate` methods to make them available through this component's `ref`\n    const euiAnimate = useCallback(() => {\n      bellRef.current?.euiAnimate();\n      cheerRef.current?.euiAnimate();\n    }, []);\n\n    // we're using the `useImperativeHandle` which allows the child to expose a function to the parent\n    // this way we can trigger the animations on both child components: `bellRef` and `cheerRef`\n    useImperativeHandle(\n      ref,\n      () => ({\n        euiAnimate,\n      }),\n      [euiAnimate]\n    );\n\n    const bellButton = (\n      <EuiHeaderSectionItemButton\n        ref={bellRef}\n        aria-label={`News feed: ${\n          showNotification ? 'Updates available' : 'No updates'\n        }`}\n        notification={showNotification}>\n        <EuiIcon type=\"bell\" />\n      </EuiHeaderSectionItemButton>\n    );\n\n    const cheerButton = (\n      <EuiHeaderSectionItemButton\n        ref={cheerRef}\n        aria-label={`News feed: ${\n          showNotification ? 'Updates available' : 'No updates'\n        }`}\n        notification={showNotification && notificationsNumber}>\n        <EuiIcon type=\"cheer\" />\n      </EuiHeaderSectionItemButton>\n    );\n\n    return (\n      <>\n        {bellButton}\n        {cheerButton}\n      </>\n    );\n  }\n);\nHeaderUpdates.displayName = 'HeaderUpdates';\n\nexport default () => {\n  const [showNotification, setShowNotification] = useState(false);\n  const headerUpdatesRef = useRef();\n  const [notificationsNumber, setNotificationsNumber] = useState(0);\n\n  const notify = () => {\n    if (!showNotification) {\n      setNotificationsNumber(1);\n      setShowNotification(true);\n    } else {\n      setNotificationsNumber(notificationsNumber + 1);\n    }\n\n    headerUpdatesRef.current?.euiAnimate();\n  };\n\n  return (\n    <>\n      <EuiFlexGroup responsive={false} alignItems=\"center\" gutterSize=\"m\">\n        <EuiFlexItem grow={false}>\n          <EuiButton size=\"s\" onClick={notify}>\n            Notify & animate\n          </EuiButton>\n        </EuiFlexItem>\n\n        <EuiFlexItem grow={false}>\n          <EuiButton\n            size=\"s\"\n            color=\"warning\"\n            onClick={() => {\n              setShowNotification(false);\n              setNotificationsNumber(0);\n            }}>\n            Reset\n          </EuiButton>\n        </EuiFlexItem>\n      </EuiFlexGroup>\n\n      <EuiSpacer />\n\n      <EuiHeader\n        sections={[\n          {\n            items: [<EuiHeaderLogo>Elastic</EuiHeaderLogo>],\n            borders: 'none',\n          },\n          {\n            items: [\n              <HeaderUpdates\n                ref={headerUpdatesRef}\n                showNotification={showNotification}\n                setShowNotification={setShowNotification}\n                notificationsNumber={notificationsNumber}\n              />,\n            ],\n            borders: 'none',\n          },\n        ]}\n      />\n    </>\n  );\n};\n"]}]}
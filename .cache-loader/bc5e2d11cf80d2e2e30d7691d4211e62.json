s={options}\n        selectedOptions={selectedOptions}\n        inputRef={setInputRef}\n        onChange={onChange}\n        onSearchChange={onSearchChange}\n        onBlur={onBlur}\n      />\n    </EuiFormRow>\n  );\n};\n"]}]}
el.Outer\n        className={classes}\n        style={styles}\n        hasShadow={false}\n        borderRadius=\"none\"\n        color=\"transparent\"\n        hasBorder={false}\n        {...rest}>\n        <figure>\n          <EuiSplitPanel.Inner\n            className={classNames('guideRule__example__panel', {\n              'guideRule__example__panel--flex': panelDisplay === 'flex',\n            })}\n            style={panelStyles}\n            color={panelColor}\n            {...panelProps}>\n            {children}\n          </EuiSplitPanel.Inner>\n          <EuiSplitPanel.Inner color=\"transparent\">\n            <EuiText color={type === 'do' ? 'success' : 'danger'} size=\"s\">\n              <p>\n                {doOrDont && <strong>{doOrDont}.</strong>} {text}\n              </p>\n            </EuiText>\n          </EuiSplitPanel.Inner>\n        </figure>\n      </EuiSplitPanel.Outer>\n    </EuiFlexItem>\n  );\n};\n\nGuideRuleExample.propTypes = {\n  children: PropTypes.node,\n  className: PropTypes.node,\n  type: PropTypes.string.isRequired,\n  text: PropTypes.node,\n  minHeight: PropTypes.number,\n  panelProps: PropTypes.any,\n};\n\nGuideRuleExample.defaultProps = {\n  type: 'do',\n};\n"]}]}
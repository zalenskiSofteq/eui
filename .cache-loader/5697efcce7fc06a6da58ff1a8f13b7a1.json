oBAAC,SAAD;AAAW,IAAA,IAAI,EAAC;AAAhB,IA3BF,eA4BE,oBAAC,OAAD;AAAS,IAAA,KAAK,EAAC,SAAf;AAAyB,IAAA,OAAO,EAAE;AAAA,aAAMA,UAAU,CAAC,GAAD,EAAM,IAAN,CAAhB;AAAA;AAAlC,2BACO,6CADP,kBAC2C,GAD3C,eAEE,wDAFF,CA5BF,eAgCE,oBAAC,SAAD;AAAW,IAAA,IAAI,EAAC;AAAhB,IAhCF,eAiCE,oBAAC,OAAD;AAAS,IAAA,KAAK,EAAC,QAAf;AAAwB,IAAA,OAAO,EAAE;AAAA,aAAMA,UAAU,CAAC,GAAD,EAAM,GAAN,CAAhB;AAAA;AAAjC,2BACO,6CADP,kBAC2C,GAD3C,eAEE,+DAFF,2DAjCF,eAsCE,oBAAC,SAAD;AAAW,IAAA,IAAI,EAAC;AAAhB,IAtCF,eAuCE,oBAAC,OAAD;AAAS,IAAA,KAAK,EAAC,SAAf;AAAyB,IAAA,OAAO,EAAE;AAAA,aAAMA,UAAU,CAAC,GAAD,EAAM,GAAN,CAAhB;AAAA;AAAlC,2BACO,6CADP,kBAC2C,GAD3C,eAEE,8DAFF,CAvCF,eA4CE,oBAAC,SAAD,OA5CF,eA8CE,oBAAC,OAAD;AAAS,IAAA,KAAK,EAAC,SAAf;AAAyB,IAAA,OAAO,EAAE;AAAA,aAAMA,UAAU,CAAC,GAAD,CAAhB;AAAA;AAAlC,2BACO,4CADP,gCAC0C,mDAD1C,CA9CF,eAiDE,oBAAC,SAAD;AAAW,IAAA,IAAI,EAAC;AAAhB,IAjDF,eAkDE,oBAAC,OAAD;AAAS,IAAA,KAAK,EAAC,SAAf;AAAyB,IAAA,OAAO,EAAE;AAAA,aAAMA,UAAU,CAAC,GAAD,EAAM,IAAN,CAAhB;AAAA;AAAlC,2BACO,4CADP,kBAC0C,GAD1C,eAEE,wDAFF,CAlDF,eAsDE,oBAAC,SAAD;AAAW,IAAA,IAAI,EAAC;AAAhB,IAtDF,eAuDE,oBAAC,OAAD;AAAS,IAAA,KAAK,EAAC,QAAf;AAAwB,IAAA,OAAO,EAAE;AAAA,aAAMA,UAAU,CAAC,GAAD,EAAM,GAAN,CAAhB;AAAA;AAAjC,2BACO,4CADP,kBAC0C,GAD1C,eAEE,+DAFF,2DAvDF,eA4DE,oBAAC,SAAD;AAAW,IAAA,IAAI,EAAC;AAAhB,IA5DF,eA6DE,oBAAC,OAAD;AAAS,IAAA,KAAK,EAAC,SAAf;AAAyB,IAAA,OAAO,EAAE;AAAA,aAAMA,UAAU,CAAC,GAAD,EAAM,IAAN,CAAhB;AAAA;AAAlC,2BACO,4CADP,kBAC0C,GAD1C,eAEE,8DAFF,CA7DF,eAkEE,oBAAC,SAAD,OAlEF,eAoEE,oBAAC,OAAD;AAAS,IAAA,KAAK,EAAC,SAAf;AAAyB,IAAA,OAAO,EAAE;AAAA,aAAMA,UAAU,CAAC,GAAD,CAAhB;AAAA;AAAlC,2BACO,0CADP,gCACwC,mDADxC,CApEF,eAuEE,oBAAC,SAAD;AAAW,IAAA,IAAI,EAAC;AAAhB,IAvEF,eAwEE,oBAAC,OAAD;AAAS,IAAA,KAAK,EAAC,SAAf;AAAyB,IAAA,OAAO,EAAE;AAAA,aAAMA,UAAU,CAAC,GAAD,EAAM,IAAN,CAAhB;AAAA;AAAlC,2BACO,0CADP,gCACwC,wDADxC,CAxEF,eA2EE,oBAAC,SAAD;AAAW,IAAA,IAAI,EAAC;AAAhB,IA3EF,eA4EE,oBAAC,OAAD;AAAS,IAAA,KAAK,EAAC,QAAf;AAAwB,IAAA,OAAO,EAAE;AAAA,aAAMA,UAAU,CAAC,GAAD,EAAM,GAAN,CAAhB;AAAA;AAAjC,2BACO,0CADP,kBACwC,GADxC,eAEE,+DAFF,uDA5EF,eAiFE,oBAAC,SAAD;AAAW,IAAA,IAAI,EAAC;AAAhB,IAjFF,eAkFE,oBAAC,OAAD;AAAS,IAAA,KAAK,EAAC,SAAf;AAAyB,IAAA,OAAO,EAAE;AAAA,aAAMA,UAAU,CAAC,GAAD,EAAM,IAAN,CAAhB;AAAA;AAAlC,2BACO,0CADP,kBACwC,GADxC,eAEE,8DAFF,CAlFF,eAuFE,oBAAC,SAAD,OAvFF,eAyFE,oBAAC,OAAD;AAAS,IAAA,KAAK,EAAC,SAAf;AAAyB,IAAA,OAAO,EAAE;AAAA,aAAMA,UAAU,CAAC,GAAD,EAAM,CAAN,CAAhB;AAAA;AAAlC,uCACmB,6CADnB,kBACuD,GADvD,eAEE,qDAFF,CAzFF,EA8FGG,MA9FH,CADF;AAkGD,CApLD","sourcesContent":["import React, { useState } from 'react';\n\nimport {\n  EuiFlyout,\n  EuiFlyoutBody,\n  EuiFlyoutHeader,\n  EuiLink,\n  EuiText,\n  EuiTitle,\n  EuiFieldText,\n  EuiForm,\n  EuiFormRow,\n  EuiFilePicker,\n  EuiRange,\n  EuiSelect,\n  EuiSpacer,\n} from '../../../../src/components';\n\nexport default () => {\n  const [isFlyoutVisible, setIsFlyoutVisible] = useState(false);\n  const [flyoutSize, setFlyoutSize] = useState('m');\n  const [flyoutMaxWidth, setFlyoutMaxWidth] = useState(false);\n\n  const closeFlyout = () => setIsFlyoutVisible(false);\n\n  const showFlyout = (size = 'm', maxWidth = false) => {\n    setFlyoutSize(size);\n    setFlyoutMaxWidth(maxWidth);\n    setIsFlyoutVisible(true);\n  };\n\n  let flyout;\n\n  if (isFlyoutVisible) {\n    let maxWidthTitle;\n    switch (flyoutMaxWidth) {\n      case true:\n        maxWidthTitle = 'Default';\n        break;\n      case false:\n        maxWidthTitle = 'No';\n        break;\n      default:\n        maxWidthTitle = `${flyoutMaxWidth}px`;\n        break;\n    }\n\n    flyout = (\n      <EuiFlyout\n        ownFocus\n        onClose={closeFlyout}\n        aria-labelledby=\"flyoutMaxWidthTitle\"\n        size={flyoutSize}\n        maxWidth={flyoutMaxWidth}>\n        <EuiFlyoutHeader hasBorder>\n          <EuiTitle size=\"m\">\n            <h2 id=\"flyoutMaxWidthTitle\">{maxWidthTitle} maxWidth</h2>\n          </EuiTitle>\n        </EuiFlyoutHeader>\n        <EuiFlyoutBody>\n          <EuiText>\n            <p>\n              In many cases, you&rsquo;ll want to set a custom width\n              that&rsquo;s tailored to your content. In this case, the flyout is\n              an ideal width for form elements.\n            </p>\n          </EuiText>\n\n          <EuiSpacer />\n\n          <EuiForm component=\"form\">\n            <EuiFormRow\n              label=\"Text field\"\n              helpText=\"I am some friendly help text.\">\n              <EuiFieldText name=\"first\" />\n            </EuiFormRow>\n\n            <EuiFormRow label=\"Select (with no initial selection)\">\n              <EuiSelect\n                hasNoInitialSelection\n                options={[\n                  { value: 'option_one', text: 'Option one' },\n                  { value: 'option_two', text: 'Option two' },\n                  { value: 'option_three', text: 'Option three' },\n                ]}\n              />\n            </EuiFormRow>\n\n            <EuiFormRow label=\"File picker\">\n              <EuiFilePicker />\n            </EuiFormRow>\n\n            <EuiFormRow label=\"Range\">\n              <EuiRange min={0} max={100} name=\"range\" id=\"range\" />\n            </EuiFormRow>\n          </EuiForm>\n        </EuiFlyoutBody>\n      </EuiFlyout>\n    );\n  }\n  return (\n    <div>\n      <EuiLink color=\"success\" onClick={() => showFlyout('s')}>\n        Show <strong>small</strong> flyout with <strong>no max-width</strong>\n      </EuiLink>\n      <EuiSpacer size=\"s\" />\n      <EuiLink color=\"success\" onClick={() => showFlyout('s', true)}>\n        Show <strong>small</strong> flyout with{' '}\n        <strong>default max-width</strong>\n      </EuiLink>\n      <EuiSpacer size=\"s\" />\n      <EuiLink color=\"danger\" onClick={() => showFlyout('s', 200)}>\n        Show <strong>small</strong> flyout with{' '}\n        <strong>smaller custom max-width</strong> -- minWidth wins except for on\n        small screens\n      </EuiLink>\n      <EuiSpacer size=\"s\" />\n      <EuiLink color=\"danger\" onClick={() => showFlyout('s', 448)}>\n        Show <strong>small</strong> flyout with{' '}\n        <strong>larger custom max-width</strong> -- minWidth wins except for on\n        small screens\n      </EuiLink>\n\n      <EuiSpacer />\n\n      <EuiLink color=\"success\" onClick={() => showFlyout('m')}>\n        Show <strong>medium</strong> flyout with <strong>no max-width</strong>\n      </EuiLink>\n      <EuiSpacer size=\"s\" />\n      <EuiLink color=\"success\" onClick={() => showFlyout('m', true)}>\n        Show <strong>medium</strong> flyout with{' '}\n        <strong>default max-width</strong>\n      </EuiLink>\n      <EuiSpacer size=\"s\" />\n      <EuiLink color=\"danger\" onClick={() => showFlyout('m', 448)}>\n        Show <strong>medium</strong> flyout with{' '}\n        <strong>smaller custom max-width</strong> -- minWidth wins and full\n        100vw wins on small screens\n      </EuiLink>\n      <EuiSpacer size=\"s\" />\n      <EuiLink color=\"success\" onClick={() => showFlyout('m', 900)}>\n        Show <strong>medium</strong> flyout with{' '}\n        <strong>larger custom max-width</strong>\n      </EuiLink>\n\n      <EuiSpacer />\n\n      <EuiLink color=\"success\" onClick={() => showFlyout('l')}>\n        Show <strong>large</strong> flyout with <strong>no max-width</strong>\n      </EuiLink>\n      <EuiSpacer size=\"s\" />\n      <EuiLink color=\"success\" onClick={() => showFlyout('l', true)}>\n        Show <strong>large</strong> flyout with{' '}\n        <strong>default max-width</strong>\n      </EuiLink>\n      <EuiSpacer size=\"s\" />\n      <EuiLink color=\"danger\" onClick={() => showFlyout('l', 448)}>\n        Show <strong>large</strong> flyout with{' '}\n        <strong>smaller custom max-width</strong> -- minWidth wins and full\n        100vw wins on small screens\n      </EuiLink>\n      <EuiSpacer size=\"s\" />\n      <EuiLink color=\"success\" onClick={() => showFlyout('l', 1600)}>\n        Show <strong>large</strong> flyout with{' '}\n        <strong>larger custom max-width</strong>\n      </EuiLink>\n\n      <EuiSpacer />\n\n      <EuiLink color=\"success\" onClick={() => showFlyout(240)}>\n        Show <strong>240</strong> flyout with <strong>no max-width</strong>\n      </EuiLink>\n      <EuiSpacer size=\"s\" />\n      <EuiLink color=\"success\" onClick={() => showFlyout(240, true)}>\n        Show <strong>240</strong> flyout with <strong>default max-width</strong>\n      </EuiLink>\n      <EuiSpacer size=\"s\" />\n      <EuiLink color=\"danger\" onClick={() => showFlyout(240, 110)}>\n        Show <strong>240</strong> flyout with{' '}\n        <strong>smaller custom max-width</strong> -- max-width wins but width\n        wins on small screens\n      </EuiLink>\n      <EuiSpacer size=\"s\" />\n      <EuiLink color=\"success\" onClick={() => showFlyout(240, 1600)}>\n        Show <strong>240</strong> flyout with{' '}\n        <strong>larger custom max-width</strong>\n      </EuiLink>\n\n      <EuiSpacer />\n\n      <EuiLink color=\"primary\" onClick={() => showFlyout('m', 0)}>\n        Trick for forms: <strong>Medium</strong> flyout with{' '}\n        <strong>0 as max-width</strong>\n      </EuiLink>\n\n      {flyout}\n    </div>\n  );\n};\n"]}]}
A,UAAU,CAACD,OAAD,CAAV;AACD,GAFD;;AAIA,MAAMG,QAAQ,GAAG,SAAXA,QAAW,CAACC,CAAD,EAAO;AACtBlB,IAAAA,mBAAmB,CAACkB,CAAC,CAACC,aAAF,CAAgBC,OAAjB,CAAnB;AACD,GAFD;;AAIA,MAAMC,mBAAmB,GAAG,SAAtBA,mBAAsB,CAACC,MAAD,EAASC,WAAT,EAAyB;AACnD,wBACE,uDACE,oBAAC,YAAD;AAAc,MAAA,MAAM,EAAEA;AAAtB,OAAoCD,MAAM,CAAClB,KAA3C,CADF,eAEE,+BAFF,eAGE,oBAAC,YAAD;AAAc,MAAA,KAAK,EAAC;AAApB,oBACE,gDACE,oBAAC,YAAD;AAAc,MAAA,MAAM,EAAEmB;AAAtB,uCADF,CADF,CAHF,CADF;AAaD,GAdD;;AAgBA,MAAIC,WAAJ;;AACA,MAAIzB,gBAAJ,EAAsB;AACpByB,IAAAA,WAAW,GAAG;AACZC,MAAAA,MAAM,EAAE,GADI;AAEZC,MAAAA,YAAY,EAAEL,mBAFF;AAGZM,MAAAA,SAAS,EAAE;AACTC,QAAAA,SAAS,EAAE,EADF;AAETjB,QAAAA,SAAS,EAAE;AAFF;AAHC,KAAd;AAQD;;AAED,sBACE,uDACE,oBAAC,SAAD;AACE,IAAA,KAAK,EAAC,gBADR;AAEE,IAAA,OAAO,EAAEZ,gBAFX;AAGE,IAAA,QAAQ,EAAEkB;AAHZ,IADF,eAOE,oBAAC,SAAD,OAPF,eASE,oBAAC,aAAD;AACE,kBAAW,2CADb;AAEE,IAAA,UAAU,MAFZ;AAGE,IAAA,OAAO,EAAEH,OAHX;AAIE,IAAA,QAAQ,EAAEE;AAJZ,KAKMQ,WALN,GAMG,UAACK,IAAD,EAAOC,MAAP;AAAA,wBACC,0CACGA,MADH,EAEGD,IAFH,CADD;AAAA,GANH,CATF,CADF;AAyBD,CAjFD","sourcesContent":["import React, { useState } from 'react';\n\nimport {\n  EuiBadge,\n  EuiHighlight,\n  EuiSpacer,\n  EuiTextColor,\n  EuiSwitch,\n} from '../../../../src/components';\nimport { EuiSelectable } from '../../../../src/components/selectable';\nimport { createDataStore } from '../tables/data_store';\n\nexport default () => {\n  const [useCustomContent, setUseCustomContent] = useState(false);\n\n  const countries = createDataStore().countries.map((country) => {\n    return {\n      label: `${country.name}`,\n      searchableLabel: `${country.name} ${'I am secondary content, I am!'}`,\n      prepend: country.flag,\n      append: <EuiBadge>{country.code}</EuiBadge>,\n      showIcons: false,\n    };\n  });\n\n  countries.unshift({\n    label: 'Country options',\n    isGroupLabel: true,\n  });\n\n  const [options, setOptions] = useState(countries);\n\n  const onChange = (options) => {\n    setOptions(options);\n  };\n\n  const onCustom = (e) => {\n    setUseCustomContent(e.currentTarget.checked);\n  };\n\n  const renderCountryOption = (option, searchValue) => {\n    return (\n      <>\n        <EuiHighlight search={searchValue}>{option.label}</EuiHighlight>\n        <br />\n        <EuiTextColor color=\"subdued\">\n          <small>\n            <EuiHighlight search={searchValue}>\n              I am secondary content, I am!\n            </EuiHighlight>\n          </small>\n        </EuiTextColor>\n      </>\n    );\n  };\n\n  let customProps;\n  if (useCustomContent) {\n    customProps = {\n      height: 240,\n      renderOption: renderCountryOption,\n      listProps: {\n        rowHeight: 50,\n        showIcons: false,\n      },\n    };\n  }\n\n  return (\n    <>\n      <EuiSwitch\n        label=\"Custom content\"\n        checked={useCustomContent}\n        onChange={onCustom}\n      />\n\n      <EuiSpacer />\n\n      <EuiSelectable\n        aria-label=\"Selectable example with custom list items\"\n        searchable\n        options={options}\n        onChange={onChange}\n        {...customProps}>\n        {(list, search) => (\n          <>\n            {search}\n            {list}\n          </>\n        )}\n      </EuiSelectable>\n    </>\n  );\n};\n"]}]}
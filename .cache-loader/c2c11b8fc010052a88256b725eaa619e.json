AD;AACE,IAAA,QAAQ,EAAC,WADX;AAEE,IAAA,OAAO,EAAEF,aAFX;AAGE,IAAA,UAAU,EAAEX,aAHd;AAIE,IAAA,UAAU,EAAE,EAJd;AAKE,IAAA,gBAAgB,EAAE,IALpB;AAME,IAAA,gBAAgB,EAAE;AANpB,iBADF;AAYA,sBACE,oBAAC,cAAD;AAAgB,IAAA,SAAS,EAAE;AAA3B,kBACE,oBAAC,eAAD;AACE,IAAA,IAAI,EAAE,KADR;AAEE,IAAA,gBAAgB,EAAEE,UAFpB;AAGE,IAAA,OAAO,EAAEM;AAHX,cADF,eAOE,oBAAC,eAAD;AACE,IAAA,QAAQ,MADV;AAEE,IAAA,IAAI,EAAE,KAFR;AAGE,IAAA,gBAAgB,EAAEJ,YAHpB;AAIE,IAAA,OAAO,EAAEK;AAJX,UAPF,eAcE,oBAAC,eAAD;AACE,IAAA,IAAI,EAAE,KADR;AAEE,IAAA,gBAAgB,EAAEH,aAFpB;AAGE,IAAA,OAAO,EAAEI;AAHX,WAdF,eAoBE,oBAAC,UAAD;AACE,IAAA,EAAE,EAAC,SADL;AAEE,IAAA,MAAM,EAAEG,MAFV;AAGE,IAAA,MAAM,EAAEb,aAHV;AAIE,IAAA,YAAY,EAAEY,YAJhB;AAKE,IAAA,gBAAgB,EAAC;AALnB,kBAME;AAAK,IAAA,SAAS,EAAC;AAAf,kBACE;AAAK,IAAA,SAAS,EAAC;AAAf,kBACE,oBAAC,OAAD;AAAS,IAAA,IAAI,EAAC;AAAd,IADF,eAEE,oBAAC,SAAD;AAAW,IAAA,IAAI,EAAC;AAAhB,IAFF,eAGE,kDAHF,CADF,CANF,CApBF,eAkCE,oBAAC,eAAD;AACE,IAAA,UAAU,EAAE,EADd;AAEE,IAAA,gBAAgB,EAAEV,UAFpB;AAGE,IAAA,OAAO,EAAEM;AAHX,oCAlCF,CADF;AA2CD,CAnFD","sourcesContent":["import React, { useState } from 'react';\n\nimport {\n  EuiPopover,\n  EuiFilterGroup,\n  EuiFilterButton,\n  EuiIcon,\n  EuiSpacer,\n} from '../../../../src/components';\n\nexport default () => {\n  const [isPopoverOpen, setIsPopoverOpen] = useState(false);\n  const [isFilterOn, setIsFilterOn] = useState(false);\n  const [isOnFilterOn, setIsOnFilterOn] = useState(false);\n  const [isOffFilterOn, setIsOffFilterOn] = useState(false);\n\n  const toggleFilter = () => {\n    setIsFilterOn(!isFilterOn);\n  };\n\n  const toggleOnFilter = () => {\n    setIsOnFilterOn(!isOnFilterOn);\n    setIsOffFilterOn(isOffFilterOn && !isOnFilterOn ? false : isOffFilterOn);\n  };\n\n  const toggleOffFilter = () => {\n    setIsOffFilterOn(!isOffFilterOn);\n    setIsOnFilterOn(isOnFilterOn && !isOffFilterOn ? false : isOnFilterOn);\n  };\n\n  const onButtonClick = () => {\n    setIsPopoverOpen(!isPopoverOpen);\n  };\n\n  const closePopover = () => {\n    setIsPopoverOpen(false);\n  };\n\n  const button = (\n    <EuiFilterButton\n      iconType=\"arrowDown\"\n      onClick={onButtonClick}\n      isSelected={isPopoverOpen}\n      numFilters={12}\n      hasActiveFilters={true}\n      numActiveFilters={2}>\n      Composers\n    </EuiFilterButton>\n  );\n\n  return (\n    <EuiFilterGroup fullWidth={true}>\n      <EuiFilterButton\n        grow={false}\n        hasActiveFilters={isFilterOn}\n        onClick={toggleFilter}>\n        Filter\n      </EuiFilterButton>\n      <EuiFilterButton\n        withNext\n        grow={false}\n        hasActiveFilters={isOnFilterOn}\n        onClick={toggleOnFilter}>\n        On\n      </EuiFilterButton>\n      <EuiFilterButton\n        grow={false}\n        hasActiveFilters={isOffFilterOn}\n        onClick={toggleOffFilter}>\n        Off\n      </EuiFilterButton>\n      <EuiPopover\n        id=\"popover\"\n        button={button}\n        isOpen={isPopoverOpen}\n        closePopover={closePopover}\n        panelPaddingSize=\"none\">\n        <div className=\"euiFilterSelect__note\">\n          <div className=\"euiFilterSelect__noteContent\">\n            <EuiIcon type=\"minusInCircle\" />\n            <EuiSpacer size=\"xs\" />\n            <p>No filters found</p>\n          </div>\n        </div>\n      </EuiPopover>\n      <EuiFilterButton\n        numFilters={12}\n        hasActiveFilters={isFilterOn}\n        onClick={toggleFilter}>\n        Filter with a very long name\n      </EuiFilterButton>\n    </EuiFilterGroup>\n  );\n};\n"]}]}
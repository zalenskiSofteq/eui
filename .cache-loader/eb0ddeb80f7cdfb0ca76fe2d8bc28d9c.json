AACwC,UAAN,CAAiBF,WAAjB,CAAhB;AACA,uBAAUC,OAAO,CAACE,IAAlB,cAA0BF,OAAO,CAACjB,IAAlC;AACD;AANH,GAnDc,EA2Dd;AACED,IAAAA,KAAK,EAAE,QADT;AAEEC,IAAAA,IAAI,EAAE,QAFR;AAGEc,IAAAA,QAAQ,EAAE,SAHZ;AAIEL,IAAAA,MAAM,EAAE,gBAAClB,MAAD;AAAA,aAAYD,YAAY,CAACC,MAAD,CAAxB;AAAA;AAJV,GA3Dc,CAAhB;AAmEA,MAAM6B,UAAU,GAAG;AACjBxC,IAAAA,SAAS,EAATA,SADiB;AAEjBE,IAAAA,QAAQ,EAARA,QAFiB;AAGjBe,IAAAA,cAAc,EAAdA,cAHiB;AAIjBwB,IAAAA,eAAe,EAAE,CAAC,CAAD,EAAI,CAAJ,EAAO,CAAP,CAJA;AAKjBC,IAAAA,kBAAkB,EAAE,CAACtC;AALJ,GAAnB;AAQA,sBACE,8CACE,oBAAC,SAAD;AACE,IAAA,OAAO,EAAE,CAACA,kBADZ;AAEE,IAAA,KAAK,eACH,gEAC6B,GAD7B,eAEE,oBAAC,OAAD,+CAFF,CAHJ;AAQE,IAAA,QAAQ,EAAEU;AARZ,IADF,eAWE,oBAAC,SAAD;AAAW,IAAA,IAAI,EAAC;AAAhB,IAXF,eAYE,oBAAC,aAAD;AACE,IAAA,KAAK,EAAEE,WADT;AAEE,IAAA,OAAO,EAAEE,OAFX;AAGE,IAAA,UAAU,EAAEsB,UAHd;AAIE,IAAA,QAAQ,EAAElC;AAJZ,IAZF,CADF;AAqBD,CAtHM","sourcesContent":["import React, { useState } from 'react';\nimport { formatDate } from '../../../../../src/services/format';\nimport { createDataStore } from '../data_store';\n\nimport {\n  EuiBasicTable,\n  EuiCode,\n  EuiLink,\n  EuiHealth,\n  EuiFlexGroup,\n  EuiFlexItem,\n  EuiSpacer,\n  EuiSwitch,\n} from '../../../../../src/components';\n\n/*\nExample user object:\n\n{\n  id: '1',\n  firstName: 'john',\n  lastName: 'doe',\n  github: 'johndoe',\n  dateOfBirth: Date.now(),\n  nationality: 'NL',\n  online: true\n}\n\nExample country object:\n\n{\n  code: 'NL',\n  name: 'Netherlands',\n  flag: 'ðŸ‡³ðŸ‡±'\n}\n*/\n\nconst store = createDataStore();\n\nexport const Table = () => {\n  const [pageIndex, setPageIndex] = useState(0);\n  const [pageSize, setPageSize] = useState(5);\n  const [showPerPageOptions, setShowPerPageOptions] = useState(true);\n\n  const onTableChange = ({ page = {} }) => {\n    const { index: pageIndex, size: pageSize } = page;\n\n    setPageIndex(pageIndex);\n    setPageSize(pageSize);\n  };\n\n  const renderStatus = (online) => {\n    const color = online ? 'success' : 'danger';\n    const label = online ? 'Online' : 'Offline';\n    return <EuiHealth color={color}>{label}</EuiHealth>;\n  };\n\n  const togglePerPageOptions = () => setShowPerPageOptions(!showPerPageOptions);\n\n  const { pageOfItems, totalItemCount } = store.findUsers(pageIndex, pageSize);\n\n  const columns = [\n    {\n      field: 'firstName',\n      name: 'First Name',\n      truncateText: true,\n      hideForMobile: true,\n      mobileOptions: {\n        show: false,\n      },\n    },\n    {\n      field: 'lastName',\n      name: 'Last Name',\n      truncateText: true,\n      mobileOptions: {\n        show: false,\n      },\n    },\n    {\n      field: 'firstName',\n      name: 'Full Name',\n      mobileOptions: {\n        header: false,\n        only: true,\n        enlarge: true,\n        fullWidth: true,\n      },\n      render: (name, item) => (\n        <EuiFlexGroup responsive={false} alignItems=\"center\">\n          <EuiFlexItem>\n            {item.firstName} {item.lastName}\n          </EuiFlexItem>\n          <EuiFlexItem grow={false}>{renderStatus(item.online)}</EuiFlexItem>\n        </EuiFlexGroup>\n      ),\n    },\n    {\n      field: 'github',\n      name: 'Github',\n      render: (username) => (\n        <EuiLink href={`https://github.com/${username}`} target=\"_blank\">\n          {username}\n        </EuiLink>\n      ),\n    },\n    {\n      field: 'dateOfBirth',\n      name: 'Date of Birth',\n      dataType: 'date',\n      render: (date) => formatDate(date, 'dobLong'),\n    },\n    {\n      field: 'nationality',\n      name: 'Nationality',\n      render: (countryCode) => {\n        const country = store.getCountry(countryCode);\n        return `${country.flag} ${country.name}`;\n      },\n    },\n    {\n      field: 'online',\n      name: 'Online',\n      dataType: 'boolean',\n      render: (online) => renderStatus(online),\n    },\n  ];\n\n  const pagination = {\n    pageIndex,\n    pageSize,\n    totalItemCount,\n    pageSizeOptions: [3, 5, 8],\n    hidePerPageOptions: !showPerPageOptions,\n  };\n\n  return (\n    <div>\n      <EuiSwitch\n        checked={!showPerPageOptions}\n        label={\n          <span>\n            Hide per page options with{' '}\n            <EuiCode>pagination.hidePerPageOptions = true</EuiCode>\n          </span>\n        }\n        onChange={togglePerPageOptions}\n      />\n      <EuiSpacer size=\"xl\" />\n      <EuiBasicTable\n        items={pageOfItems}\n        columns={columns}\n        pagination={pagination}\n        onChange={onTableChange}\n      />\n    </div>\n  );\n};\n"]}]}
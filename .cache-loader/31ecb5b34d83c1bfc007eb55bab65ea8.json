,QAAQ,EAAE,KAPZ;AAQE1B,IAAAA,OAAO,eACL,8CACE,oBAAC,SAAD,OADF,eAEE,oBAAC,WAAD,eACMe,eADN;AAEE,MAAA,OAAO,eACL,8CACE,qDADF,eAEE,oBAAC,SAAD,OAFF,eAGE,oBAAC,SAAD;AAAW,QAAA,KAAK,EAAC,SAAjB;AAA2B,QAAA,OAAO,EAAEO;AAApC,iCAHF;AAHJ,qBAWE,8CACE,oBAAC,OAAD;AAAS,MAAA,KAAK,EAAC,QAAf;AAAwB,MAAA,WAAW,EAAC;AAApC,MADF,CAXF,CAFF;AATJ,GAvDW,CAAb;AAsFA,sBACE,oBAAC,eAAD;AACE,IAAA,UAAU,EAAE;AACVO,MAAAA,SAAS,EAAE,QADD;AAEVC,MAAAA,cAAc,EAAE,cACd,oBAAC,cAAD,CAAgB,QAAhB,QACG;AAAA,YAAGC,UAAH,QAAGA,UAAH;AAAA,4BACC,oBAAC,SAAD;AAAW,UAAA,IAAI,MAAf;AAAgB,UAAA,IAAI,aAAMA,UAAN,CAApB;AAAwC,UAAA,QAAQ,EAAC;AAAjD,mCADD;AAAA,OADH,CADc,CAFN;AAWVP,MAAAA,IAAI,EAAEA,IAAI,CAACQ,GAAL,CAAS,UAACC,GAAD,EAAMC,KAAN,EAAgB;AAC7B,eAAO;AACLC,UAAAA,GAAG,EAAED,KADA;AAELE,UAAAA,KAAK,EAAEH,GAAG,CAACR,IAFN;AAGLJ,UAAAA,EAAE,EAAEY,GAAG,CAACZ,EAHH;AAILgB,UAAAA,OAAO,EAAE;AAAA,mBAAMjB,UAAU,CAACa,GAAG,CAACZ,EAAL,CAAhB;AAAA,WAJJ;AAKLiB,UAAAA,UAAU,EAAEL,GAAG,CAACZ,EAAJ,KAAWX;AALlB,SAAP;AAOD,OARK;AAXI;AADd,KAsBGc,IAAI,CAACQ,GAAL,CAAS,UAACC,GAAD,EAAMC,KAAN;AAAA,wBACR,oBAAC,QAAD;AAAU,MAAA,GAAG,EAAEA;AAAf,OACGD,GAAG,CAACZ,EAAJ,KAAWX,aAAX,iBACC;AAAK,MAAA,IAAI,EAAC,UAAV;AAAqB,yBAAiBuB,GAAG,CAACZ;AAA1C,OACGY,GAAG,CAACjC,OADP,CAFJ,CADQ;AAAA,GAAT,CAtBH,CADF;AAkCD,CApJD","sourcesContent":["import React, { Fragment, useState } from 'react';\n\nimport {\n  EuiButton,\n  EuiButtonEmpty,\n  EuiColorPicker,\n  EuiColorPickerSwatch,\n  EuiPageTemplate,\n  EuiSpacer,\n  EuiStat,\n  EuiTextArea,\n  EuiTourStep,\n  useEuiTour,\n} from '../../../../src/components';\nimport { ExampleContext } from '../../services';\n\nconst demoTourSteps = [\n  {\n    step: 1,\n    title: 'Step 1',\n  },\n  {\n    step: 2,\n    title: 'Step 2',\n    anchorPosition: 'upCenter',\n    content: <p>What is your favorite color?</p>,\n  },\n  {\n    step: 3,\n    title: 'Step 3',\n    content: <p>Click here for more cool things.</p>,\n    anchorPosition: 'downRight',\n    minWidth: 'auto',\n  },\n  {\n    step: 4,\n    title: 'Step 4',\n    anchorPosition: 'downLeft',\n    decoration: 'none',\n  },\n];\n\nconst tourConfig = {\n  currentTourStep: 1,\n  isTourActive: true,\n  tourPopoverWidth: true,\n  tourSubtitle: 'Demo tour',\n};\n\nexport default () => {\n  const [color, setColor] = useState('#000');\n  const [selectedTabId, setSelectedTabId] = useState('query');\n  const [\n    [euiTourStepOne, euiTourStepTwo, euiTourStepThree, euiTourStepFour],\n    actions,\n    reducerState,\n  ] = useEuiTour(demoTourSteps, tourConfig);\n\n  const onSelectColor = (color) => {\n    setColor(color);\n    if (reducerState.currentTourStep === 2) {\n      actions.goToStep(3);\n    }\n  };\n\n  const onTabClick = (id) => {\n    if (id === 'stat' && reducerState.currentTourStep === 3) {\n      actions.goToStep(4);\n    }\n    setSelectedTabId(id);\n  };\n\n  const onReset = () => {\n    actions.resetTour();\n    setSelectedTabId('query');\n  };\n\n  const tabs = [\n    {\n      id: 'query',\n      name: 'Query',\n      disabled: false,\n      content: (\n        <div>\n          <EuiSpacer />\n          <EuiTourStep\n            {...euiTourStepOne}\n            content={\n              <div>\n                <p>This is a neat thing. You enter queries here.</p>\n                <EuiSpacer />\n                <EuiButton color=\"primary\" onClick={actions.incrementStep}>\n                  Ok, got it.\n                </EuiButton>\n              </div>\n            }>\n            <EuiTextArea\n              placeholder=\"Placeholder text\"\n              aria-label=\"Enter ES SQL query\"\n              defaultValue=\"{queryValue}\"\n              style={{ width: 400 }}\n            />\n          </EuiTourStep>\n\n          <EuiSpacer />\n\n          <EuiTourStep\n            {...euiTourStepTwo}\n            footerAction={\n              <EuiButtonEmpty\n                color=\"text\"\n                flush=\"right\"\n                size=\"xs\"\n                onClick={actions.incrementStep}>\n                {\"I don't have a favorite color\"}\n              </EuiButtonEmpty>\n            }>\n            <EuiColorPicker\n              onChange={onSelectColor}\n              color={color}\n              mode=\"swatch\"\n              button={\n                <EuiColorPickerSwatch\n                  color={color}\n                  aria-label=\"Select a color\"\n                />\n              }\n            />\n          </EuiTourStep>\n        </div>\n      ),\n    },\n    {\n      id: 'stat',\n      name: (\n        <EuiTourStep {...euiTourStepThree}>\n          <span>Stats</span>\n        </EuiTourStep>\n      ),\n      disabled: false,\n      content: (\n        <div>\n          <EuiSpacer />\n          <EuiTourStep\n            {...euiTourStepFour}\n            content={\n              <div>\n                <p>That about does it.</p>\n                <EuiSpacer />\n                <EuiButton color=\"primary\" onClick={onReset}>\n                  Take me to the start.\n                </EuiButton>\n              </div>\n            }>\n            <div>\n              <EuiStat title=\"22,123\" description=\"Queries\" />\n            </div>\n          </EuiTourStep>\n        </div>\n      ),\n    },\n  ];\n\n  return (\n    <EuiPageTemplate\n      pageHeader={{\n        pageTitle: 'My app',\n        rightSideItems: [\n          <ExampleContext.Consumer>\n            {({ parentPath }) => (\n              <EuiButton fill href={`#${parentPath}`} iconType=\"exit\">\n                Exit full screen demo\n              </EuiButton>\n            )}\n          </ExampleContext.Consumer>,\n        ],\n        tabs: tabs.map((tab, index) => {\n          return {\n            key: index,\n            label: tab.name,\n            id: tab.id,\n            onClick: () => onTabClick(tab.id),\n            isSelected: tab.id === selectedTabId,\n          };\n        }),\n      }}>\n      {tabs.map((tab, index) => (\n        <Fragment key={index}>\n          {tab.id === selectedTabId && (\n            <div role=\"tabpanel\" aria-labelledby={tab.id}>\n              {tab.content}\n            </div>\n          )}\n        </Fragment>\n      ))}\n    </EuiPageTemplate>\n  );\n};\n"]}]}
,EAAC;AAAhB,IA3BF,eA6BE,oBAAC,WAAD;AACE,IAAA,EAAE,EAAEN,eAAe,IADrB;AAEE,IAAA,KAAK,EAAC,4BAFR;AAGE,IAAA,OAAO,EAAEC,OAHX;AAIE,IAAA,QAAQ,EAAE,kBAACK,CAAD;AAAA,aAAOD,SAAQ,CAACC,CAAD,CAAf;AAAA,KAJZ;AAKE,IAAA,UAAU;AALZ,IA7BF,CADF;AAuCD,CAnDD","sourcesContent":["import React, { useState, Fragment } from 'react';\n\nimport { EuiCheckbox, EuiSpacer } from '../../../../src/components';\n\nimport { htmlIdGenerator } from '../../../../src/services';\n\nexport default () => {\n  const [checked, setChecked] = useState(false);\n  const [indeterminate, setindeterminate] = useState(true);\n\n  const onChange = (e) => {\n    setChecked(e.target.checked);\n  };\n\n  const onChangeIndeterminate = () => {\n    setindeterminate(!indeterminate);\n  };\n\n  return (\n    <Fragment>\n      <EuiCheckbox\n        id={htmlIdGenerator()()}\n        label=\"I am a checkbox\"\n        checked={checked}\n        onChange={(e) => onChange(e)}\n      />\n\n      <EuiSpacer size=\"m\" />\n\n      <EuiCheckbox\n        id={htmlIdGenerator()()}\n        label=\"I am an indeterminate checkbox\"\n        indeterminate={indeterminate}\n        onChange={() => onChangeIndeterminate()}\n      />\n\n      <EuiSpacer size=\"m\" />\n\n      <EuiCheckbox\n        id={htmlIdGenerator()()}\n        label=\"I am a disabled checkbox\"\n        checked={checked}\n        onChange={(e) => onChange(e)}\n        disabled\n      />\n\n      <EuiSpacer size=\"m\" />\n\n      <EuiCheckbox\n        id={htmlIdGenerator()()}\n        label=\"I am a compressed checkbox\"\n        checked={checked}\n        onChange={(e) => onChange(e)}\n        compressed\n      />\n    </Fragment>\n  );\n};\n"]}]}
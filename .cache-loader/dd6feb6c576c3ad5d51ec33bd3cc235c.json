,IAAAA,KAAK,EAAE,6CADT;AAEEC,IAAAA,QAAQ,eACN,oBAAC,QAAD,qBACE,iFADF,eAEE,oBAAC,SAAD,OAFF,EAGGL,cAHH,OAGoBC,aAHpB,OAGoCC,YAHpC,CAHJ;AASEJ,IAAAA,MAAM,EAAEA;AATV,GALsB,CAAxB;AAkBA,sBACE,8CACE,oBAAC,QAAD;AAAU,IAAA,KAAK,EAAEK;AAAjB,IADF,CADF;AAKD,CArED","sourcesContent":["import React, { useState, Fragment } from 'react';\n\nimport { EuiSpacer, EuiSteps, EuiButton } from '../../../../src/components';\n\nexport default () => {\n  const [status, setStatus] = useState('incomplete');\n\n  let completeButton;\n  if (status !== 'complete') {\n    completeButton = (\n      <EuiButton onClick={() => setStatus('complete')}>\n        You complete me\n      </EuiButton>\n    );\n  } else {\n    completeButton = (\n      <EuiButton onClick={() => setStatus('incomplete')}>Reset</EuiButton>\n    );\n  }\n\n  let warningButton;\n  if (status !== 'warning') {\n    warningButton = (\n      <EuiButton color=\"warning\" onClick={() => setStatus('warning')}>\n        Uh oh!\n      </EuiButton>\n    );\n  } else {\n    warningButton = (\n      <EuiButton color=\"warning\" onClick={() => setStatus('incomplete')}>\n        Reset\n      </EuiButton>\n    );\n  }\n\n  let dangerButton;\n  if (status !== 'danger') {\n    dangerButton = (\n      <EuiButton color=\"danger\" onClick={() => setStatus('danger')}>\n        Something terrible\n      </EuiButton>\n    );\n  } else {\n    dangerButton = (\n      <EuiButton color=\"danger\" onClick={() => setStatus('incomplete')}>\n        Reset\n      </EuiButton>\n    );\n  }\n\n  const firstSetOfSteps = [\n    {\n      title: 'Normal step',\n      children: <p>Do this first</p>,\n    },\n    {\n      title: 'Push the button to complete this final step',\n      children: (\n        <Fragment>\n          <p>We are fancy buttons just waiting to be pushed!</p>\n          <EuiSpacer />\n          {completeButton} {warningButton} {dangerButton}\n        </Fragment>\n      ),\n      status: status,\n    },\n  ];\n\n  return (\n    <div>\n      <EuiSteps steps={firstSetOfSteps} />\n    </div>\n  );\n};\n"]}]}
ible] = useState(false);\n  const [isPopoverOpen, setIsPopoverOpen] = useState(false);\n\n  const closeModal = () => {\n    setIsModalVisible(false);\n  };\n\n  const showModal = () => {\n    setIsModalVisible(true);\n  };\n\n  const togglePopover = () => {\n    setIsPopoverOpen(!isPopoverOpen);\n  };\n\n  const closePopover = () => {\n    setIsPopoverOpen(false);\n  };\n\n  const colorPicker = <EuiColorPicker color={color} onChange={setColor} />;\n\n  const stops = (\n    <EuiColorStops\n      label=\"Color stops\"\n      onChange={setColorStops}\n      colorStops={colorStops}\n      min={0}\n      max={100}\n    />\n  );\n\n  const button = (\n    <EuiButton iconType=\"arrowDown\" iconSide=\"right\" onClick={togglePopover}>\n      Open popover\n    </EuiButton>\n  );\n\n  let modal;\n\n  if (isModalVisible) {\n    modal = (\n      <EuiModal onClose={closeModal} style={{ width: '800px' }}>\n        <EuiModalHeader>\n          <EuiModalHeaderTitle>\n            <h1>Color picker in a modal</h1>\n          </EuiModalHeaderTitle>\n        </EuiModalHeader>\n\n        <EuiModalBody>\n          <EuiFormRow label=\"Color picker\">{colorPicker}</EuiFormRow>\n          <EuiSpacer />\n          <EuiFormRow label=\"Color stops\">{stops}</EuiFormRow>\n        </EuiModalBody>\n      </EuiModal>\n    );\n  }\n\n  return (\n    <Fragment>\n      <EuiFormRow\n        label=\"Color picker\"\n        helpText=\"This color picker is inside of a form row\">\n        {colorPicker}\n      </EuiFormRow>\n\n      <EuiSpacer />\n\n      <EuiFormRow\n        label=\"Color stops\"\n        helpText=\"This color stops component is inside of a form row\">\n        {stops}\n      </EuiFormRow>\n\n      <EuiFormRow label=\"Unruly focus management\">\n        <EuiPopover\n          id=\"popover\"\n          button={button}\n          isOpen={isPopoverOpen}\n          closePopover={closePopover}>\n          <div style={{ width: '300px' }}>\n            <EuiFormRow label=\"Color picker\">{colorPicker}</EuiFormRow>\n            <EuiSpacer />\n            <EuiFormRow label=\"Color stops\">{stops}</EuiFormRow>\n          </div>\n        </EuiPopover>\n      </EuiFormRow>\n\n      <EuiSpacer size=\"m\" />\n\n      <EuiButton onClick={showModal}>Show modal</EuiButton>\n\n      {modal}\n    </Fragment>\n  );\n};\n"]}]}
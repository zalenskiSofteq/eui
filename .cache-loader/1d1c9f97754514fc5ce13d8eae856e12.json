C,cAAD;AACE,MAAA,UAAU,MADZ;AAEE,MAAA,KAAK,EAAElB,QAAQ,CAACJ,KAFlB;AAGE,MAAA,QAAQ,EAAEa;AAHZ,MADF,CAdF,CADF,CADqB;AAAA,GAAvB;;AA2BA,sBACE,oBAAC,YAAD;AAAc,IAAA,UAAU,EAAC;AAAzB,kBACE,oBAAC,WAAD;AAAa,IAAA,IAAI,EAAE;AAAnB,kBACE,oBAAC,UAAD;AACE,IAAA,EAAE,EAAC,UADL;AAEE,IAAA,MAAM,eACJ,oBAAC,aAAD;AACE,MAAA,WAAW,EAAC,MADd;AAEE,MAAA,KAAK,EAAEZ,QAAQ,CAACD,KAFlB;AAGE,MAAA,QAAQ,EAAEC,QAAQ,CAACF,MAHrB;AAIE,MAAA,OAAO,EAAEO;AAJX,MAHJ;AAUE,IAAA,MAAM,EAAEL,QAAQ,CAACF,MAVnB;AAWE,IAAA,YAAY,EAAEQ,aAXhB;AAYE,IAAA,gBAAgB,EAAC,GAZnB;AAaE,IAAA,cAAc,EAAC;AAbjB,KAcGY,cAAc,EAdjB,CADF,CADF,eAoBE,oBAAC,WAAD;AAAa,IAAA,IAAI,EAAE;AAAnB,kBACE,oBAAC,UAAD;AACE,IAAA,EAAE,EAAC,UADL;AAEE,IAAA,gBAAgB,EAAC,GAFnB;AAGE,IAAA,MAAM,eACJ,oBAAC,aAAD;AACE,MAAA,WAAW,EAAEf,QAAQ,CAACD,WADxB;AAEE,MAAA,KAAK,EAAEC,QAAQ,CAACJ,KAFlB;AAGE,MAAA,QAAQ,EAAEI,QAAQ,CAACL,MAHrB;AAIE,MAAA,OAAO,EAAES;AAJX,MAJJ;AAWE,IAAA,MAAM,EAAEJ,QAAQ,CAACL,MAXnB;AAYE,IAAA,YAAY,EAAEU,aAZhB;AAaE,IAAA,cAAc,EAAC;AAbjB,KAcGY,cAAc,EAdjB,CADF,CApBF,CADF;AAyCD,CA5JD","sourcesContent":["import React, { useState } from 'react';\n\nimport {\n  EuiPopoverTitle,\n  EuiFlexItem,\n  EuiFlexGroup,\n  EuiPopover,\n  EuiSelect,\n  EuiFieldNumber,\n  EuiExpression,\n} from '../../../../src/components';\n\n// Rise the popovers above GuidePageSideNav\nconst POPOVER_STYLE = { zIndex: '200' };\n\nexport default () => {\n  const [example1, setExample1] = useState({\n    isOpen: false,\n    value: 'count()',\n  });\n\n  const [example2, setExample2] = useState({\n    value: 100,\n    description: 'Is above',\n  });\n\n  const openExample1 = () => {\n    setExample1({\n      ...example1,\n      isOpen: true,\n    });\n    setExample2({\n      ...example2,\n      isOpen: false,\n    });\n  };\n\n  const closeExample1 = () => {\n    setExample1({\n      ...example1,\n      isOpen: false,\n    });\n  };\n\n  const openExample2 = () => {\n    setExample1({\n      ...example1,\n      isOpen: false,\n    });\n    setExample2({\n      ...example2,\n      isOpen: true,\n    });\n  };\n\n  const closeExample2 = () => {\n    setExample2({\n      ...example2,\n      isOpen: false,\n    });\n  };\n\n  const changeExample1 = (event) => {\n    setExample1({\n      ...example1,\n      value: event.target.value,\n    });\n  };\n\n  const changeExample2Value = (e) => {\n    const sanitizedValue = parseInt(e.target.value, 10);\n    setExample2({\n      ...example2,\n      value: isNaN(sanitizedValue) ? '' : sanitizedValue,\n    });\n  };\n\n  const changeExample2Description = (event) => {\n    setExample2({\n      ...example2,\n      description: event.target.value,\n    });\n  };\n\n  const renderPopover1 = () => (\n    <div style={POPOVER_STYLE}>\n      <EuiPopoverTitle>When</EuiPopoverTitle>\n      <EuiSelect\n        compressed\n        value={example1.value}\n        onChange={changeExample1}\n        options={[\n          { value: 'count()', text: 'count()' },\n          { value: 'average()', text: 'average()' },\n          { value: 'sum()', text: 'sum()' },\n          { value: 'median()', text: 'median()' },\n          { value: 'min()', text: 'min()' },\n          { value: 'max()', text: 'max()' },\n        ]}\n      />\n    </div>\n  );\n\n  const renderPopover2 = () => (\n    <div style={POPOVER_STYLE}>\n      <EuiFlexGroup gutterSize=\"s\">\n        <EuiFlexItem grow={false} style={{ width: 150 }}>\n          <EuiSelect\n            compressed\n            value={example2.description}\n            onChange={changeExample2Description}\n            options={[\n              { value: 'Is above', text: 'Is above' },\n              { value: 'Is below', text: 'Is below' },\n              { value: 'Is exactly', text: 'Is exactly' },\n            ]}\n          />\n        </EuiFlexItem>\n\n        <EuiFlexItem grow={false} style={{ width: 100 }}>\n          <EuiFieldNumber\n            compressed\n            value={example2.value}\n            onChange={changeExample2Value}\n          />\n        </EuiFlexItem>\n      </EuiFlexGroup>\n    </div>\n  );\n\n  return (\n    <EuiFlexGroup gutterSize=\"s\">\n      <EuiFlexItem grow={false}>\n        <EuiPopover\n          id=\"popover1\"\n          button={\n            <EuiExpression\n              description=\"when\"\n              value={example1.value}\n              isActive={example1.isOpen}\n              onClick={openExample1}\n            />\n          }\n          isOpen={example1.isOpen}\n          closePopover={closeExample1}\n          panelPaddingSize=\"s\"\n          anchorPosition=\"downLeft\">\n          {renderPopover1()}\n        </EuiPopover>\n      </EuiFlexItem>\n\n      <EuiFlexItem grow={false}>\n        <EuiPopover\n          id=\"popover2\"\n          panelPaddingSize=\"s\"\n          button={\n            <EuiExpression\n              description={example2.description}\n              value={example2.value}\n              isActive={example2.isOpen}\n              onClick={openExample2}\n            />\n          }\n          isOpen={example2.isOpen}\n          closePopover={closeExample2}\n          anchorPosition=\"downLeft\">\n          {renderPopover2()}\n        </EuiPopover>\n      </EuiFlexItem>\n    </EuiFlexGroup>\n  );\n};\n"]}]}
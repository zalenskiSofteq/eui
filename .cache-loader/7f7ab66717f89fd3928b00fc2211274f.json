CAACD,KAAK,GAAG,CAAT,CAAd;AAAA,KAApB;AAA+C,IAAA,IAAI,EAAC;AAApD,sBA7DF,CADF;AAmED,CAtED","sourcesContent":["import React, { useState } from 'react';\n\nimport {\n  EuiButton,\n  EuiI18n,\n  EuiMark,\n  EuiSpacer,\n  EuiTitle,\n  useEuiI18n,\n} from '../../../../src/components';\n\nexport default () => {\n  const [count, setCount] = useState(1);\n\n  return (\n    <>\n      <EuiTitle size=\"xs\">\n        <h3>useEuiI18n with string interpolation</h3>\n      </EuiTitle>\n      <p>\n        {useEuiI18n(\n          'euiI18nInterpolation.clickedCount',\n          'Clicked on button {count} times.',\n          {\n            count,\n          }\n        )}\n      </p>\n\n      <EuiSpacer size=\"l\" />\n\n      <EuiTitle size=\"xs\">\n        <h3>EuiI18n with string interpolation</h3>\n      </EuiTitle>\n      <p>\n        <EuiI18n\n          token=\"euiI18nInterpolation.clickedCount\"\n          default=\"Clicked on button {count} times.\"\n          values={{\n            count,\n          }}\n        />\n      </p>\n\n      <EuiSpacer size=\"l\" />\n\n      <EuiTitle size=\"xs\">\n        <h3>useEuiI18n with component interpolation</h3>\n      </EuiTitle>\n      <p>\n        {useEuiI18n(\n          'euiI18nInterpolation.clickedCount',\n          'Clicked on button {count} times.',\n          {\n            count: <EuiMark color=\"primary\">{count}</EuiMark>,\n          }\n        )}\n      </p>\n\n      <EuiSpacer size=\"l\" />\n\n      <EuiTitle size=\"xs\">\n        <h3>EuiI18n with component interpolation</h3>\n      </EuiTitle>\n      <p>\n        <EuiI18n\n          token=\"euiI18nInterpolation.clickedCount\"\n          default=\"Clicked on button {count} times.\"\n          values={{\n            count: <EuiMark color=\"primary\">{count}</EuiMark>,\n          }}\n        />\n      </p>\n\n      <EuiSpacer size=\"l\" />\n\n      <EuiButton onClick={() => setCount(count + 1)} size=\"s\">\n        Increase count\n      </EuiButton>\n    </>\n  );\n};\n"]}]}
;AA1ImB,mBAkJyB7B,QAAQ,CAClDwB,OAAO,CAACa,GAAR,CAAY;AAAA,QAAGZ,EAAH,SAAGA,EAAH;AAAA,WAAYA,EAAZ;AAAA,GAAZ,CADkD,CAlJjC;AAAA;AAAA,MAkJZa,cAlJY;AAAA,MAkJIC,iBAlJJ;;AAAA,mBAsJiBvC,QAAQ,CAAC;AAC3CwC,IAAAA,QAAQ,EAAE,CADiC;AAE3CC,IAAAA,SAAS,EAAE,CAFgC;AAG3CC,IAAAA,eAAe,EAAE,CAAC,CAAD;AAH0B,GAAD,CAtJzB;AAAA;AAAA,MAsJZC,UAtJY;AAAA,MAsJAC,aAtJA;;AA2JnB,MAAMC,oBAAoB,GAAG5C,WAAW,CACtC,UAACuC,QAAD;AAAA,WAAcI,aAAa,CAAC,UAACD,UAAD;AAAA,6CAAsBA,UAAtB;AAAkCH,QAAAA,QAAQ,EAARA;AAAlC;AAAA,KAAD,CAA3B;AAAA,GADsC,EAEtC,CAACI,aAAD,CAFsC,CAAxC;AAIA,MAAME,YAAY,GAAG7C,WAAW,CAC9B,UAACwC,SAAD;AAAA,WACEG,aAAa,CAAC,UAACD,UAAD;AAAA,6CAAsBA,UAAtB;AAAkCF,QAAAA,SAAS,EAATA;AAAlC;AAAA,KAAD,CADf;AAAA,GAD8B,EAG9B,CAACG,aAAD,CAH8B,CAAhC;AAMA,sBACE,uDACE,oBAAC,SAAD;AACE,IAAA,KAAK,EAAC,oFADR;AAEE,IAAA,OAAO,EAAE1B,qBAFX;AAGE,IAAA,QAAQ,EAAEE;AAHZ,IADF,eAOE,oBAAC,SAAD,OAPF,eASE,oBAAC,WAAD;AACE,IAAA,GAAG,EAAEF,qBAAqB,GAAG,qBAAH,GAA2B,gBADvD;AAEE,kBAAW,sBAFb;AAGE,IAAA,OAAO,EAAEM,OAHX;AAIE,IAAA,gBAAgB,EAAE;AAAEc,MAAAA,cAAc,EAAdA,cAAF;AAAkBC,MAAAA,iBAAiB,EAAjBA;AAAlB,KAJpB;AAKE,IAAA,QAAQ,EAAEzB,IAAI,CAACiC,MALjB;AAME,IAAA,eAAe,EAAEd,eANnB;AAOE,IAAA,UAAU,kCACLU,UADK;AAERE,MAAAA,oBAAoB,EAApBA,oBAFQ;AAGRC,MAAAA,YAAY,EAAZA;AAHQ;AAPZ,IATF,CADF;AAyBD,CA9LD","sourcesContent":["/* eslint-disable jsx-a11y/accessible-emoji */\nimport React, { useState, useCallback, useMemo } from 'react';\nimport { fake } from 'faker';\n\nimport {\n  EuiDataGrid,\n  EuiButtonEmpty,\n  EuiButtonIcon,\n  EuiLink,\n  EuiSwitch,\n  EuiSpacer,\n  EuiBadge,\n  EuiToken,\n  EuiFlexGroup,\n  EuiFlexItem,\n} from '../../../../src/components/';\n\nconst data = [];\n\nfor (let i = 0; i < 10; i++) {\n  data.push([\n    <span>{fake('{{name.firstName}}')}</span>,\n    <span>{fake('{{name.firstName}}')}</span>,\n\n    <span>\n      <EuiLink href=\"#/tabular-content/data-grid-focus\">\n        {fake('{{internet.email}}')}\n      </EuiLink>\n    </span>,\n    <span>\n      <EuiLink href=\"#/tabular-content/data-grid-focus\">\n        {fake('{{internet.email}}')}\n      </EuiLink>\n    </span>,\n\n    <span>\n      <EuiButtonEmpty size=\"xs\" onClick={() => {}}>\n        Yes\n      </EuiButtonEmpty>\n      <EuiButtonEmpty size=\"xs\" color=\"danger\" onClick={() => {}}>\n        No\n      </EuiButtonEmpty>\n    </span>,\n    <span>\n      <EuiButtonEmpty size=\"xs\" onClick={() => {}}>\n        Yes\n      </EuiButtonEmpty>\n      <EuiButtonEmpty size=\"xs\" color=\"danger\" onClick={() => {}}>\n        No\n      </EuiButtonEmpty>\n    </span>,\n  ]);\n}\n\nconst renderHeaderIcon = (areHeadersInteractive) =>\n  areHeadersInteractive ? (\n    <EuiFlexItem grow={false}>\n      <EuiButtonIcon\n        aria-label=\"column settings\"\n        iconType=\"gear\"\n        onClick={() => {}}\n      />\n    </EuiFlexItem>\n  ) : null;\n\nexport default () => {\n  const [areHeadersInteractive, setAreHeadersInteractive] = useState(false);\n  const switchInteractiveHeaders = useCallback(\n    (e) => setAreHeadersInteractive(e.target.checked),\n    [setAreHeadersInteractive]\n  );\n\n  const columns = useMemo(\n    () => [\n      {\n        id: 'no-interactives not expandable',\n        display: (\n          <EuiFlexGroup alignItems=\"center\" gutterSize=\"xs\" responsive={false}>\n            {renderHeaderIcon(areHeadersInteractive)}\n            <EuiFlexItem grow={false}>\n              <EuiToken\n                iconType=\"expandMini\"\n                color=\"euiColorVis2\"\n                shape=\"square\"\n                fill=\"dark\"\n              />\n            </EuiFlexItem>\n            <EuiFlexItem grow={false}>\n              <EuiBadge>0 interactive</EuiBadge>\n            </EuiFlexItem>\n          </EuiFlexGroup>\n        ),\n        isExpandable: false,\n        actions: false,\n      },\n      {\n        id: 'no-interactives is expandable',\n        display: (\n          <EuiFlexGroup alignItems=\"center\" gutterSize=\"xs\" responsive={false}>\n            <EuiFlexItem grow={false}>\n              <EuiToken\n                iconType=\"expandMini\"\n                color=\"euiColorVis0\"\n                shape=\"square\"\n                fill=\"dark\"\n              />\n            </EuiFlexItem>\n            <EuiFlexItem grow={false}>\n              <EuiBadge>0 interactive</EuiBadge>\n            </EuiFlexItem>\n          </EuiFlexGroup>\n        ),\n        actions: false,\n      },\n      {\n        id: 'one-interactive not expandable',\n        display: (\n          <EuiFlexGroup alignItems=\"center\" gutterSize=\"xs\" responsive={false}>\n            <EuiFlexItem grow={false}>\n              <EuiToken\n                iconType=\"expandMini\"\n                color=\"euiColorVis2\"\n                shape=\"square\"\n                fill=\"dark\"\n              />\n            </EuiFlexItem>\n            <EuiFlexItem grow={false}>\n              <EuiBadge>1 interactive</EuiBadge>\n            </EuiFlexItem>\n          </EuiFlexGroup>\n        ),\n        isExpandable: false,\n        actions: false,\n      },\n      {\n        id: 'one-interactives is expandable',\n        display: (\n          <EuiFlexGroup alignItems=\"center\" gutterSize=\"xs\" responsive={false}>\n            {renderHeaderIcon(areHeadersInteractive)}\n            <EuiFlexItem grow={false}>\n              <EuiToken\n                iconType=\"expandMini\"\n                color=\"euiColorVis0\"\n                shape=\"square\"\n                fill=\"dark\"\n              />\n            </EuiFlexItem>\n            <EuiFlexItem grow={false}>\n              <EuiBadge>1 interactive</EuiBadge>\n            </EuiFlexItem>\n          </EuiFlexGroup>\n        ),\n        actions: false,\n      },\n      {\n        id: 'two-interactives not expandable',\n\n        display: (\n          <EuiFlexGroup alignItems=\"center\" gutterSize=\"xs\" responsive={false}>\n            <EuiFlexItem grow={false}>\n              <EuiToken\n                iconType=\"expandMini\"\n                color=\"euiColorVis2\"\n                shape=\"square\"\n                fill=\"dark\"\n              />\n            </EuiFlexItem>\n            <EuiFlexItem grow={false}>\n              <EuiBadge>2 interactive</EuiBadge>\n            </EuiFlexItem>\n          </EuiFlexGroup>\n        ),\n        isExpandable: false,\n        actions: false,\n      },\n      {\n        id: 'two-interactives is expandable',\n\n        display: (\n          <EuiFlexGroup alignItems=\"center\" gutterSize=\"xs\" responsive={false}>\n            <EuiFlexItem grow={false}>\n              <EuiToken\n                iconType=\"expandMini\"\n                color=\"euiColorVis0\"\n                shape=\"square\"\n                fill=\"dark\"\n              />\n            </EuiFlexItem>\n            <EuiFlexItem grow={false}>\n              <EuiBadge>2 interactive</EuiBadge>\n            </EuiFlexItem>\n          </EuiFlexGroup>\n        ),\n        actions: false,\n      },\n    ],\n    [areHeadersInteractive]\n  );\n  const columnIdToIndex = columns.reduce((acc, { id }, index) => {\n    acc[id] = index;\n    return acc;\n  }, {});\n\n  const renderCellValue = useCallback(\n    ({ rowIndex, columnId }) => {\n      const columnIndex = columnIdToIndex[columnId];\n      return data[rowIndex][columnIndex];\n    },\n    [columnIdToIndex]\n  );\n\n  const [visibleColumns, setVisibleColumns] = useState(\n    columns.map(({ id }) => id)\n  );\n\n  const [pagination, setPagination] = useState({\n    pageSize: 4,\n    pageIndex: 0,\n    pageSizeOptions: [4],\n  });\n  const onChangeItemsPerPage = useCallback(\n    (pageSize) => setPagination((pagination) => ({ ...pagination, pageSize })),\n    [setPagination]\n  );\n  const onChangePage = useCallback(\n    (pageIndex) =>\n      setPagination((pagination) => ({ ...pagination, pageIndex })),\n    [setPagination]\n  );\n\n  return (\n    <>\n      <EuiSwitch\n        label=\"Use interactive headers - toggling will reset the datagrid and any internal states\"\n        checked={areHeadersInteractive}\n        onChange={switchInteractiveHeaders}\n      />\n\n      <EuiSpacer />\n\n      <EuiDataGrid\n        key={areHeadersInteractive ? 'interactive-headers' : 'static-headers'}\n        aria-label=\"Top EUI contributors\"\n        columns={columns}\n        columnVisibility={{ visibleColumns, setVisibleColumns }}\n        rowCount={data.length}\n        renderCellValue={renderCellValue}\n        pagination={{\n          ...pagination,\n          onChangeItemsPerPage,\n          onChangePage,\n        }}\n      />\n    </>\n  );\n};\n"]}]}
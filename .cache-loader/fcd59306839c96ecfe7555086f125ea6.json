,eAAe,IAAIA,eAAe,CAACc,MAAhB,GAAyB,CAA5C,GAAgD,KAAhD,GAAwD,IAL5D;AAOE,MAAA,QAAQ,EAAEpB,QAAQ,CAACF,MAPrB;AAQE,MAAA,OAAO,EAAEU;AARX,MAHJ;AAcE,IAAA,MAAM,EAAER,QAAQ,CAACF,MAdnB;AAeE,IAAA,YAAY,EAAEW,aAfhB;AAgBE,IAAA,OAAO,EAAC,OAhBV;AAiBE,IAAA,gBAAgB,EAAC,GAjBnB;AAkBE,IAAA,cAAc,EAAC;AAlBjB,KAmBGY,cAAc,EAnBjB,CADF,eAuBE,oBAAC,UAAD;AACE,IAAA,EAAE,EAAC,iBADL;AAEE,IAAA,gBAAgB,EAAC,GAFnB;AAGE,IAAA,MAAM,eACJ,oBAAC,aAAD;AACE,MAAA,WAAW,EAAC,MADd;AAEE,MAAA,OAAO,EAAC,SAFV;AAGE,MAAA,KAAK,EAAEnB,QAAQ,CAACH,KAHlB;AAIE,MAAA,QAAQ,EAAEG,QAAQ,CAACJ,MAJrB;AAKE,MAAA,OAAO,EAAEY;AALX,MAJJ;AAYE,IAAA,MAAM,EAAER,QAAQ,CAACJ,MAZnB;AAaE,IAAA,YAAY,EAAEa,aAbhB;AAcE,IAAA,OAAO,EAAC,OAdV;AAeE,IAAA,cAAc,EAAC;AAfjB,KAgBGY,cAAc,EAhBjB,CAvBF,eAyCE,oBAAC,aAAD;AACE,IAAA,OAAO,EAAC,SADV;AAEE,IAAA,WAAW,EAAC,QAFd;AAGE,IAAA,KAAK,EAAC;AAHR,IAzCF,eA8CE,oBAAC,SAAD,OA9CF,eA+CE,oBAAC,QAAD;AAAU,IAAA,IAAI,EAAC;AAAf,kBACE,4DADF,CA/CF,eAkDE,oBAAC,aAAD;AACE,IAAA,WAAW,EAAC,MADd;AAEE,IAAA,OAAO,EAAC,SAFV;AAGE,IAAA,gBAAgB,EAAE,EAHpB;AAIE,IAAA,KAAK,EAAC,2BAJR;AAKE,IAAA,OAAO,EAAE,mBAAM,CAAE;AALnB,IAlDF,CADF;AA4DD,CA3LD","sourcesContent":["import React, { useState, Fragment } from 'react';\n\nimport {\n  EuiPopoverTitle,\n  EuiPopover,\n  EuiSelect,\n  EuiComboBox,\n  EuiExpression,\n  EuiTitle,\n  EuiSpacer,\n} from '../../../../src/components';\n\nexport default () => {\n  const [example1, setExample1] = useState({\n    isOpen: false,\n    value: (\n      <Fragment>\n        <p>.kibana_task_manager,</p>\n        <p>kibana_sample_data_ecommerce</p>\n      </Fragment>\n    ),\n  });\n\n  const [example2, setExample2] = useState({\n    isOpen: false,\n    value: 'count()',\n  });\n\n  const options = [\n    {\n      label: '.kibana_task_manager',\n    },\n    {\n      label: 'kibana_sample_data_ecommerce',\n    },\n    {\n      label: '.kibana-event-log-8.0.0-000001',\n    },\n    {\n      label: 'kibana_sample_data_flights',\n    },\n    {\n      label: '.kibana-event-log-8.0.0',\n    },\n  ];\n\n  const [selectedOptions, setSelected] = useState([options[0], options[1]]);\n\n  const openExample1 = () => {\n    setExample1({\n      ...example1,\n      isOpen: !example1.isOpen,\n    });\n  };\n\n  const closeExample1 = () => {\n    setExample1({\n      ...example1,\n      isOpen: false,\n    });\n  };\n\n  const openExample2 = () => {\n    setExample2({\n      ...example2,\n      isOpen: !example2.isOpen,\n    });\n  };\n\n  const closeExample2 = () => {\n    setExample2({\n      ...example2,\n      isOpen: false,\n    });\n  };\n\n  const changeExample2 = (e) => {\n    setExample2({\n      value: e.target.value,\n      isOpen: false,\n    });\n  };\n\n  const onChange = (selectedOptions) => {\n    setSelected(selectedOptions);\n    const indices = selectedOptions.map((s, index) => {\n      return (\n        <p key={index}>\n          {s.label}\n          {index < selectedOptions.length - 1 ? ',' : null}\n        </p>\n      );\n    });\n    setExample1({\n      ...example1,\n      value: indices,\n    });\n  };\n\n  const renderPopover1 = () => (\n    <div style={{ width: 300 }}>\n      <EuiPopoverTitle>INDICES</EuiPopoverTitle>\n      <EuiComboBox\n        placeholder=\"Select one or more indices\"\n        options={options}\n        selectedOptions={selectedOptions}\n        onChange={onChange}\n        isClearable={true}\n        data-test-subj=\"demoComboBox\"\n      />\n    </div>\n  );\n\n  const renderPopover2 = () => (\n    <div style={{ width: 150 }}>\n      <EuiPopoverTitle>WHEN</EuiPopoverTitle>\n      <EuiSelect\n        compressed\n        value={example2.value}\n        onChange={changeExample2}\n        options={[\n          {\n            value: 'count()',\n            text: 'count()',\n          },\n          {\n            value: 'sum()',\n            text: 'sum()',\n          },\n          {\n            value: 'min()',\n            text: 'min()',\n          },\n          { value: 'max()', text: 'max()' },\n        ]}\n      />\n    </div>\n  );\n\n  return (\n    <div style={{ maxWidth: 500 }}>\n      <EuiPopover\n        id=\"columnsPopover1\"\n        button={\n          <EuiExpression\n            description=\"indices\"\n            display=\"columns\"\n            value={example1.value}\n            isInvalid={\n              selectedOptions && selectedOptions.length > 0 ? false : true\n            }\n            isActive={example1.isOpen}\n            onClick={openExample1}\n          />\n        }\n        isOpen={example1.isOpen}\n        closePopover={closeExample1}\n        display=\"block\"\n        panelPaddingSize=\"s\"\n        anchorPosition=\"downLeft\">\n        {renderPopover1()}\n      </EuiPopover>\n\n      <EuiPopover\n        id=\"columnsPopover2\"\n        panelPaddingSize=\"s\"\n        button={\n          <EuiExpression\n            description=\"when\"\n            display=\"columns\"\n            value={example2.value}\n            isActive={example2.isOpen}\n            onClick={openExample2}\n          />\n        }\n        isOpen={example2.isOpen}\n        closePopover={closeExample2}\n        display=\"block\"\n        anchorPosition=\"downLeft\">\n        {renderPopover2()}\n      </EuiPopover>\n      <EuiExpression\n        display=\"columns\"\n        description=\"Except\"\n        value=\"kibana_sample_data_ky_counties\"\n      />\n      <EuiSpacer />\n      <EuiTitle size=\"xxs\">\n        <h3>Description width at 50px</h3>\n      </EuiTitle>\n      <EuiExpression\n        description=\"join\"\n        display=\"columns\"\n        descriptionWidth={50}\n        value=\"kibana_sample_data_ky_avl\"\n        onClick={() => {}}\n      />\n    </div>\n  );\n};\n"]}]}
AAI,GAAG;AACXC,IAAAA,OAAO,EAAEN,QAAQ,CAACG,QAAD,CADN;AAEXI,IAAAA,YAAY,EAAE,sBAACC,KAAD;AAAA,aAAW,IAAIC,IAAI,CAACC,YAAT,CAAsBP,QAAtB,EAAgCQ,MAAhC,CAAuCH,KAAvC,CAAX;AAAA;AAFH,GAAb;AAKA,sBACE,uDACE,oBAAC,YAAD;AAAc,IAAA,UAAU,EAAC,GAAzB;AAA6B,IAAA,UAAU,EAAC;AAAxC,kBACE,oBAAC,WAAD;AAAa,IAAA,IAAI,EAAE;AAAnB,kBACE,oBAAC,SAAD;AAAW,IAAA,IAAI,EAAEL,QAAQ,KAAK,IAA9B;AAAoC,IAAA,OAAO,EAAE;AAAA,aAAMC,WAAW,CAAC,IAAD,CAAjB;AAAA;AAA7C,kBACE,oBAAC,OAAD;AAAS,IAAA,KAAK,EAAC,oBAAf;AAAoC,IAAA,OAAO,EAAC;AAA5C,IADF,CADF,CADF,eAOE,oBAAC,WAAD;AAAa,IAAA,IAAI,EAAE;AAAnB,kBACE,oBAAC,SAAD;AAAW,IAAA,IAAI,EAAED,QAAQ,KAAK,IAA9B;AAAoC,IAAA,OAAO,EAAE;AAAA,aAAMC,WAAW,CAAC,IAAD,CAAjB;AAAA;AAA7C,kBACE,oBAAC,OAAD;AAAS,IAAA,KAAK,EAAC,mBAAf;AAAmC,IAAA,OAAO,EAAC;AAA3C,IADF,CADF,CAPF,CADF,eAeE,oBAAC,SAAD;AAAW,IAAA,IAAI,EAAC;AAAhB,IAfF,eAiBE,oBAAC,UAAD;AAAY,IAAA,IAAI,EAAEC;AAAlB,kBACE,oBAAC,eAAD,OADF,CAjBF,CADF;AAuBD,CA/BD","sourcesContent":["import React, { useState, Fragment } from 'react';\n\nimport {\n  EuiContext,\n  EuiButton,\n  EuiFieldText,\n  EuiFlexGroup,\n  EuiFlexItem,\n  EuiFormRow,\n  EuiSpacer,\n  EuiI18n,\n  EuiI18nNumber,\n  useEuiI18n,\n} from '../../../../src/components';\n\nconst mappings = {\n  fr: {\n    'euiContext.english': 'Anglais',\n    'euiContext.french': 'Française',\n    'euiContext.greeting': 'Salutations!',\n    'euiContext.guestNo': 'Vous êtes invité #',\n    'euiContext.question': 'Quel est votre nom?',\n    'euiContext.placeholder': 'Jean Dupont',\n    'euiContext.action': 'Soumettre',\n  },\n};\n\nconst ContextConsumer = () => {\n  return (\n    <div>\n      <strong>\n        <EuiI18n token=\"euiContext.greeting\" default=\"Welcome!\" />\n      </strong>\n\n      <EuiSpacer size=\"s\" />\n\n      <p>\n        <EuiI18n token=\"euiContext.guestNo\" default=\"You are guest #\" />\n        <EuiI18nNumber value={1582394} />\n      </p>\n\n      <EuiSpacer size=\"m\" />\n\n      <Fragment>\n        <EuiFormRow\n          label={useEuiI18n('euiContext.question', 'What is your name?')}>\n          <EuiFieldText\n            placeholder={useEuiI18n('euiContext.placeholder', 'John Doe')}\n          />\n        </EuiFormRow>\n\n        <EuiSpacer />\n\n        <EuiButton>{useEuiI18n('euiContext.action', 'Submit')}</EuiButton>\n      </Fragment>\n    </div>\n  );\n};\n\nexport default () => {\n  const [language, setLanguage] = useState('en');\n\n  const i18n = {\n    mapping: mappings[language],\n    formatNumber: (value) => new Intl.NumberFormat(language).format(value),\n  };\n\n  return (\n    <>\n      <EuiFlexGroup gutterSize=\"s\" alignItems=\"center\">\n        <EuiFlexItem grow={false}>\n          <EuiButton fill={language === 'en'} onClick={() => setLanguage('en')}>\n            <EuiI18n token=\"euiContext.english\" default=\"English\" />\n          </EuiButton>\n        </EuiFlexItem>\n\n        <EuiFlexItem grow={false}>\n          <EuiButton fill={language === 'fr'} onClick={() => setLanguage('fr')}>\n            <EuiI18n token=\"euiContext.french\" default=\"French\" />\n          </EuiButton>\n        </EuiFlexItem>\n      </EuiFlexGroup>\n\n      <EuiSpacer size=\"m\" />\n\n      <EuiContext i18n={i18n}>\n        <ContextConsumer />\n      </EuiContext>\n    </>\n  );\n};\n"]}]}
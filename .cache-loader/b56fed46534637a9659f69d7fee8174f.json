AC;AAAxB,IADF,CADK,gBAKL,oBAAC,QAAD,OANJ,CAhBF,CADF,CADF;AA8BD,CA5CD","sourcesContent":["import React, { useState, Fragment } from 'react';\nimport {\n  EuiDelayRender,\n  EuiFlexItem,\n  EuiCheckbox,\n  EuiFormRow,\n  EuiFieldNumber,\n  EuiLoadingSpinner,\n} from '../../../../src/components';\n\nexport default () => {\n  const [minimumDelay, setDelay] = useState(3000);\n  const [render, setRender] = useState(false);\n\n  const onChangeMinimumDelay = (event) => {\n    setDelay(parseInt(event.target.value, 10));\n  };\n\n  const onChangeHide = (event) => {\n    setRender(event.target.checked);\n  };\n\n  const status = render ? 'showing' : 'hidden';\n  const label = `Child (${status})`;\n  return (\n    <Fragment>\n      <EuiFlexItem>\n        <EuiFormRow>\n          <EuiCheckbox\n            id=\"dummy-id\"\n            checked={render}\n            onChange={onChangeHide}\n            label=\"Show child\"\n          />\n        </EuiFormRow>\n        <EuiFormRow label=\"Minimum delay\">\n          <EuiFieldNumber\n            value={minimumDelay}\n            onChange={onChangeMinimumDelay}\n          />\n        </EuiFormRow>\n\n        <EuiFormRow label={label}>\n          {render ? (\n            <EuiDelayRender delay={minimumDelay}>\n              <EuiLoadingSpinner size=\"m\" />\n            </EuiDelayRender>\n          ) : (\n            <Fragment />\n          )}\n        </EuiFormRow>\n      </EuiFlexItem>\n    </Fragment>\n  );\n};\n"]}]}
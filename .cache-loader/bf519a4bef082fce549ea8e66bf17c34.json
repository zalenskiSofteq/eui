,eAYE,oBAAC,SAAD;AACE,IAAA,OAAO,EAAE,mBAAM;AACbF,MAAAA,cAAc,CAAC,CAACD,WAAF,CAAd;AACAJ,MAAAA,YAAY,CAACQ,OAAb,CACE,oCADF,EAEEX,IAAI,CAACY,SAAL,CAAe,CAACL,WAAhB,CAFF;AAID;AAPH,iBAQWA,WAAW,GAAG,IAAH,GAAU,KARhC,CAZF,CAVF,CADF,EAoCGA,WAAW,iBACV,oBAAC,OAAD;AAAS,IAAA,IAAI,EAAC,GAAd;AAAkB,IAAA,KAAK,EAAC;AAAxB,kBACE,oDACM,oBAAC,OAAD,iBADN,mEAEwB,GAFxB,eAGE,oBAAC,OAAD;AAAS,IAAA,QAAQ,EAAC;AAAlB,iCAHF,MADF,CArCJ,CADF;AAgDD,CA5DD","sourcesContent":["import React, { useState } from 'react';\n\nimport { EuiCollapsibleNav } from '../../../../src/components/collapsible_nav';\nimport { EuiButton } from '../../../../src/components/button';\nimport { EuiTitle } from '../../../../src/components/title';\nimport { EuiSpacer } from '../../../../src/components/spacer';\nimport { EuiText } from '../../../../src/components/text';\nimport { EuiCode } from '../../../../src/components/code';\n\nexport default () => {\n  const [navIsOpen, setNavIsOpen] = useState<boolean>(\n    JSON.parse(\n      String(localStorage.getItem('euiCollapsibleNavExample--isDocked'))\n    ) || false\n  );\n  const [navIsDocked, setNavIsDocked] = useState<boolean>(\n    JSON.parse(\n      String(localStorage.getItem('euiCollapsibleNavExample--isDocked'))\n    ) || false\n  );\n\n  return (\n    <>\n      <EuiCollapsibleNav\n        isOpen={navIsOpen}\n        isDocked={navIsDocked}\n        size={240}\n        button={\n          <EuiButton onClick={() => setNavIsOpen((isOpen) => !isOpen)}>\n            Toggle nav\n          </EuiButton>\n        }\n        onClose={() => setNavIsOpen(false)}>\n        <div style={{ padding: 16 }}>\n          <EuiTitle>\n            <h2>I am some nav</h2>\n          </EuiTitle>\n          <EuiSpacer />\n          <EuiText size=\"s\" color=\"subdued\">\n            <p>\n              The docked status is being stored in{' '}\n              <EuiCode>localStorage</EuiCode>.\n            </p>\n          </EuiText>\n          <EuiSpacer />\n          <EuiButton\n            onClick={() => {\n              setNavIsDocked(!navIsDocked);\n              localStorage.setItem(\n                'euiCollapsibleNavExample--isDocked',\n                JSON.stringify(!navIsDocked)\n              );\n            }}>\n            Docked: {navIsDocked ? 'on' : 'off'}\n          </EuiButton>\n        </div>\n      </EuiCollapsibleNav>\n\n      {navIsDocked && (\n        <EuiText size=\"s\" color=\"subdued\">\n          <p>\n            The <EuiCode>button</EuiCode> gets hidden by default when the nav is\n            docked unless you set{' '}\n            <EuiCode language=\"js\">showButtonIfDocked = true</EuiCode>.\n          </p>\n        </EuiText>\n      )}\n    </>\n  );\n};\n"]}]}
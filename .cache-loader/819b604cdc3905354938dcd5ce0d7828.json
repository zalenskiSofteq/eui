 '../../../../src/components';\nimport { EuiSpacer } from '../../../../src/components/spacer';\n\nconst steps = [\n  {\n    title: 'Step 1 has intro plus code snippet',\n    children: (\n      <>\n        <EuiText>\n          <p>Run this code snippet to install things.</p>\n        </EuiText>\n        <EuiSpacer />\n        <EuiCodeBlock language=\"bash\">npm install</EuiCodeBlock>\n      </>\n    ),\n  },\n  {\n    title: 'Step 2 has sub steps',\n    children: (\n      <EuiText>\n        <p>\n          In order to complete this step, do the following things{' '}\n          <strong>in order</strong>.\n        </p>\n        <EuiSubSteps>\n          <ol>\n            <li>Do thing 1</li>\n            <li>Do thing 2</li>\n            <li>Do thing 3</li>\n          </ol>\n        </EuiSubSteps>\n        <p>Here are some bullet point reminders.</p>\n        <ul>\n          <li>Reminder 1</li>\n          <li>Reminder 2</li>\n          <li>Reminder 3</li>\n        </ul>\n      </EuiText>\n    ),\n  },\n  {\n    title: 'Step 3 has an intro and one line instruction',\n    children: (\n      <EuiText>\n        <p>\n          Now that you&apos;ve completed step 2, go find the{' '}\n          <EuiCode>thing</EuiCode>.\n        </p>\n        <p>\n          Go to <strong>Overview &gt;&gt; Endpoints</strong> note{' '}\n          <strong>Elasticsearch</strong> as <EuiCode>&lt;thing&gt;</EuiCode>.\n        </p>\n      </EuiText>\n    ),\n  },\n  {\n    title: 'The last step has two options',\n    children: (\n      <EuiText size=\"s\">\n        <h3>\n          <strong>Option 1:</strong> If you have this type of instance\n        </h3>\n        <EuiSubSteps>\n          <ol>\n            <li>Do thing 1</li>\n            <li>Do thing 2</li>\n            <li>Do thing 3</li>\n          </ol>\n        </EuiSubSteps>\n        <h3>\n          <strong>Option 2:</strong> If you have the other type of instance\n        </h3>\n        <EuiSubSteps>\n          <ol>\n            <li>Do thing 1</li>\n            <li>Do thing 2</li>\n            <li>Do thing 3</li>\n          </ol>\n        </EuiSubSteps>\n      </EuiText>\n    ),\n  },\n];\n\nexport default () => (\n  <div>\n    <EuiSteps headingElement=\"h2\" steps={steps} />\n  </div>\n);\n"]}]}
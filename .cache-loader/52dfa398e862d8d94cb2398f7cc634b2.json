AAC,kBAAD;AACE,IAAA,GAAG,EAAC,SADN;AAEE,IAAA,IAAI,EAAEH,WAAW,CAAC,EAAD,CAFnB;AAGE,IAAA,OAAO,EAAE,mBAAM;AACbH,MAAAA,YAAY;AACZH,MAAAA,UAAU,CAAC,EAAD,CAAV;AACD;AANH,eADY,eAUZ,oBAAC,kBAAD;AACE,IAAA,GAAG,EAAC,SADN;AAEE,IAAA,IAAI,EAAEM,WAAW,CAAC,EAAD,CAFnB;AAGE,IAAA,OAAO,EAAE,mBAAM;AACbH,MAAAA,YAAY;AACZH,MAAAA,UAAU,CAAC,EAAD,CAAV;AACD;AANH,eAVY,eAmBZ,oBAAC,kBAAD;AACE,IAAA,GAAG,EAAC,SADN;AAEE,IAAA,IAAI,EAAEM,WAAW,CAAC,EAAD,CAFnB;AAGE,IAAA,OAAO,EAAE,mBAAM;AACbH,MAAAA,YAAY;AACZH,MAAAA,UAAU,CAAC,EAAD,CAAV;AACD;AANH,eAnBY,eA4BZ,oBAAC,kBAAD;AACE,IAAA,GAAG,EAAC,UADN;AAEE,IAAA,IAAI,EAAEM,WAAW,CAAC,GAAD,CAFnB;AAGE,IAAA,OAAO,EAAE,mBAAM;AACbH,MAAAA,YAAY;AACZH,MAAAA,UAAU,CAAC,GAAD,CAAV;AACD;AANH,gBA5BY,CAAd;AAuCA,sBACE,oBAAC,YAAD;AAAc,IAAA,cAAc,EAAC,cAA7B;AAA4C,IAAA,UAAU,EAAC;AAAvD,kBACE,oBAAC,WAAD;AAAa,IAAA,IAAI,EAAE;AAAnB,kBACE,oBAAC,UAAD;AACE,IAAA,MAAM,EAAEQ,MADV;AAEE,IAAA,MAAM,EAAEb,aAFV;AAGE,IAAA,YAAY,EAAEQ,YAHhB;AAIE,IAAA,gBAAgB,EAAC;AAJnB,kBAKE,oBAAC,mBAAD;AAAqB,IAAA,KAAK,EAAEM;AAA5B,IALF,CADF,CADF,eAWE,oBAAC,WAAD;AAAa,IAAA,IAAI,EAAE;AAAnB,kBACE,oBAAC,aAAD;AACE,kBAAW,2BADb;AAEE,IAAA,SAAS,EAAER,UAFb;AAGE,IAAA,UAAU,EAAEJ,UAHd;AAIE,IAAA,WAAW,EAAEO;AAJf,IADF,CAXF,CADF;AAsBD,CAzFD","sourcesContent":["import React, { useState } from 'react';\n\nimport {\n  EuiButtonEmpty,\n  EuiContextMenuItem,\n  EuiContextMenuPanel,\n  EuiFlexGroup,\n  EuiFlexItem,\n  EuiPagination,\n  EuiPopover,\n} from '../../../../src/components';\n\nexport default () => {\n  const [isPopoverOpen, setIsPopoverOpen] = useState(false);\n  const [activePage, setActivePage] = useState(0);\n  const [rowSize, setRowSize] = useState(50);\n\n  const PAGE_COUNT = 10;\n\n  const onButtonClick = () =>\n    setIsPopoverOpen((isPopoverOpen) => !isPopoverOpen);\n  const closePopover = () => setIsPopoverOpen(false);\n\n  const goToPage = (pageNumber) => setActivePage(pageNumber);\n\n  const getIconType = (size) => {\n    return size === rowSize ? 'check' : 'empty';\n  };\n\n  const button = (\n    <EuiButtonEmpty\n      size=\"s\"\n      color=\"text\"\n      iconType=\"arrowDown\"\n      iconSide=\"right\"\n      onClick={onButtonClick}>\n      Rows per page: {rowSize}\n    </EuiButtonEmpty>\n  );\n\n  const items = [\n    <EuiContextMenuItem\n      key=\"10 rows\"\n      icon={getIconType(10)}\n      onClick={() => {\n        closePopover();\n        setRowSize(10);\n      }}>\n      10 rows\n    </EuiContextMenuItem>,\n    <EuiContextMenuItem\n      key=\"20 rows\"\n      icon={getIconType(20)}\n      onClick={() => {\n        closePopover();\n        setRowSize(20);\n      }}>\n      20 rows\n    </EuiContextMenuItem>,\n    <EuiContextMenuItem\n      key=\"50 rows\"\n      icon={getIconType(50)}\n      onClick={() => {\n        closePopover();\n        setRowSize(50);\n      }}>\n      50 rows\n    </EuiContextMenuItem>,\n    <EuiContextMenuItem\n      key=\"100 rows\"\n      icon={getIconType(100)}\n      onClick={() => {\n        closePopover();\n        setRowSize(100);\n      }}>\n      100 rows\n    </EuiContextMenuItem>,\n  ];\n\n  return (\n    <EuiFlexGroup justifyContent=\"spaceBetween\" alignItems=\"center\">\n      <EuiFlexItem grow={false}>\n        <EuiPopover\n          button={button}\n          isOpen={isPopoverOpen}\n          closePopover={closePopover}\n          panelPaddingSize=\"none\">\n          <EuiContextMenuPanel items={items} />\n        </EuiPopover>\n      </EuiFlexItem>\n\n      <EuiFlexItem grow={false}>\n        <EuiPagination\n          aria-label=\"Custom pagination example\"\n          pageCount={PAGE_COUNT}\n          activePage={activePage}\n          onPageClick={goToPage}\n        />\n      </EuiFlexItem>\n    </EuiFlexGroup>\n  );\n};\n"]}]}
/components';\n\nexport default () => {\n  const [isDisplaying, setDisplay] = useState(false);\n\n  const toggleDisplay = () => {\n    setDisplay(!isDisplaying);\n  };\n\n  const controls = [\n    {\n      controlType: 'icon',\n      id: 'icon',\n      iconType: 'folderClosed',\n      'aria-label': 'folder',\n      className: 'eui-hideFor--m eui-hideFor--l eui-hideFor--xl',\n    },\n    {\n      controlType: 'breadcrumbs',\n      id: 'current_file_path',\n      className: 'eui-hideFor--s eui-hideFor--xs',\n      breadcrumbs: [\n        {\n          text: 'src',\n        },\n        {\n          text: 'components',\n        },\n      ],\n    },\n    {\n      controlType: 'spacer',\n    },\n    {\n      controlType: 'icon',\n      id: 'github_icon',\n      iconType: 'logoGithub',\n      'aria-label': 'Github',\n    },\n    {\n      controlType: 'text',\n      id: 'github_text',\n      text: 'Open in Github',\n    },\n  ];\n\n  let display;\n\n  if (isDisplaying) {\n    display = <EuiControlBar controls={controls} showOnMobile />;\n  }\n\n  return (\n    <div>\n      <EuiButton onClick={toggleDisplay}>Toggle mobile example</EuiButton>\n      {display}\n    </div>\n  );\n};\n"]}]}
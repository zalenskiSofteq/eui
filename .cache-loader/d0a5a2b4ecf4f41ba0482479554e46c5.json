,KALX;AAME,IAAA,UAAU,EAAEA,OAAO,KAAK;AAN1B,4BAOsBA,OAAO,GAAG,CAPhC,WAOwCA,OAAO,GAAG,CAAV,IAAe,GAPvD,CATF,CAPF,eA0BE;AAAI,IAAA,EAAE,EAAEM;AAAR,KAAiBJ,IAAjB,CA1BF,CADF;AA8BD,CAtCD;;AAwCA,gBAAe;AAAA,sBACb,oBAAC,YAAD;AACE,IAAA,EAAE,EAAEJ,eAAe,IADrB;AAEE,IAAA,aAAa,EAAC,iCAFhB;AAGE,IAAA,aAAa,EAAE,IAHjB;AAIE,IAAA,WAAW,EAAC;AAJd,kBAKE,oBAAC,QAAD;AAAU,IAAA,KAAK,EAAC;AAAhB,kBACE,oBAAC,IAAD,OADF,CALF,CADa;AAAA,CAAf","sourcesContent":["import React, { useState } from 'react';\n\nimport {\n  EuiAccordion,\n  EuiButton,\n  EuiSpacer,\n  EuiText,\n  EuiScreenReaderOnly,\n} from '../../../../src/components';\nimport { EuiPanel } from '../../../../src/components/panel';\nimport { htmlIdGenerator } from '../../../../src/services';\n\nconst Rows = () => {\n  const [counter, setCounter] = useState(1);\n  const rows = [];\n  for (let i = 1; i <= counter; i++) {\n    rows.push(<li key={i}>Row {i}</li>);\n  }\n  const growingAccordianDescriptionId = htmlIdGenerator()();\n  const listId = htmlIdGenerator()();\n  return (\n    <EuiText size=\"s\">\n      <EuiScreenReaderOnly>\n        <p id={growingAccordianDescriptionId}>\n          Currently height is set to {counter} items\n        </p>\n      </EuiScreenReaderOnly>\n      <EuiSpacer size=\"s\" />\n      <p>\n        <EuiButton\n          size=\"s\"\n          iconType=\"plusInCircleFilled\"\n          onClick={() => setCounter(counter + 1)}\n          aria-controls={listId}\n          aria-describedby={growingAccordianDescriptionId}>\n          Increase height to {counter + 1} items\n        </EuiButton>{' '}\n        <EuiButton\n          size=\"s\"\n          iconType=\"minusInCircleFilled\"\n          aria-controls={listId}\n          aria-describedby={growingAccordianDescriptionId}\n          onClick={() => setCounter(Math.max(0, counter - 1))}\n          isDisabled={counter === 1}>\n          Decrease height to {counter - 1} item{counter > 2 && 's'}\n        </EuiButton>\n      </p>\n      <ul id={listId}>{rows}</ul>\n    </EuiText>\n  );\n};\n\nexport default () => (\n  <EuiAccordion\n    id={htmlIdGenerator()()}\n    buttonContent=\"Click me to toggle close / open\"\n    initialIsOpen={true}\n    paddingSize=\"s\">\n    <EuiPanel color=\"subdued\">\n      <Rows />\n    </EuiPanel>\n  </EuiAccordion>\n);\n"]}]}
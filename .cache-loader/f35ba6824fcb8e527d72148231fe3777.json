      const newThing = thing === second ? first : second;\n      const newType = type === 'div' ? 'span' : 'div';\n      setThing(newThing);\n      setThingAndType([newThing, newType]);\n    }, 5000);\n  }, [thing, type]);\n\n  return (\n    <EuiText size=\"s\">\n      <p>\n        <strong>Example:</strong>\n      </p>\n      <EuiInnerText>\n        {(ref, innerText) => (\n          <React.Fragment>\n            <EuiPanel paddingSize=\"s\" className=\"eui-displayInlineBlock\">\n              <span ref={ref} title={innerText}>\n                Simple string content\n              </span>\n            </EuiPanel>\n            <EuiSpacer />\n            <p className=\"eui-displayInlineBlock\">\n              <strong>Output:</strong>\n            </p>{' '}\n            <EuiCode>{innerText}</EuiCode>\n          </React.Fragment>\n        )}\n      </EuiInnerText>\n\n      <EuiHorizontalRule margin=\"xl\" />\n\n      <p>\n        <strong>Example with complex children:</strong>\n      </p>\n      <EuiInnerText>\n        {(ref, innerText) => (\n          <React.Fragment>\n            <EuiPanel paddingSize=\"s\" className=\"eui-displayInlineBlock\">\n              <span ref={ref} title={innerText}>\n                <EuiHighlight search=\"content\">\n                  EuiHighlight content\n                </EuiHighlight>{' '}\n                <EuiBadge>with EuiBadge</EuiBadge>\n              </span>\n            </EuiPanel>\n            <EuiSpacer />\n            <p className=\"eui-displayInlineBlock\">\n              <strong>Output:</strong>\n            </p>{' '}\n            <EuiCode>{innerText}</EuiCode>\n          </React.Fragment>\n        )}\n      </EuiInnerText>\n\n      <EuiHorizontalRule margin=\"xl\" />\n\n      <p>\n        <strong>Example with updating content:</strong>\n      </p>\n      <EuiInnerText>\n        {(ref, innerText) => (\n          <React.Fragment>\n            <EuiPanel paddingSize=\"s\" className=\"eui-displayInlineBlock\">\n              <span ref={ref} title={innerText}>\n                {thing}\n              </span>\n            </EuiPanel>\n            <EuiSpacer />\n            <p className=\"eui-displayInlineBlock\">\n              <strong>Output:</strong>\n            </p>{' '}\n            <EuiCode>{innerText}</EuiCode>\n          </React.Fragment>\n        )}\n      </EuiInnerText>\n\n      <EuiHorizontalRule margin=\"xl\" />\n\n      <p>\n        <strong>Example with updating element:</strong>\n      </p>\n      <EuiInnerText>\n        {(ref, innerText) => (\n          <React.Fragment>\n            <EuiPanel paddingSize=\"s\" className=\"eui-displayInlineBlock\">\n              {React.createElement(\n                type,\n                {\n                  ref,\n                  title: innerText,\n                },\n                thing2\n              )}\n            </EuiPanel>\n            <EuiSpacer />\n            <p className=\"eui-displayInlineBlock\">\n              <strong>Output:</strong>\n            </p>{' '}\n            <EuiCode>{innerText}</EuiCode>\n          </React.Fragment>\n        )}\n      </EuiInnerText>\n    </EuiText>\n  );\n};\n"]}]}
c;AACnDF,IAAAA,QAAQ,CAAC,UAACG,SAAD;AAAA,6CACJA,SADI,GAEJD,QAFI;AAAA,KAAD,CAAR;AAID,GALqC,EAKnC,EALmC,CAAtC;AAMA,MAAME,cAAc,GAAG1B,WAAW,CAAC;AAAA,WAAMsB,QAAQ,CAACJ,YAAD,CAAd;AAAA,GAAD,EAA+B,EAA/B,CAAlC;AACA,MAAMS,YAAY,GAAG3B,WAAW,CAC9B;AAAA;;AAAA,WACEsB,QAAQ,6CACLZ,YADK,EACU,EADV,8BAELC,aAFK,EAEW,EAFX,cADV;AAAA,GAD8B,EAM9B,EAN8B,CAAhC;AAQA,MAAMiB,YAAY,GAAG5B,WAAW,CAC9B;AAAA;;AAAA,WACEsB,QAAQ,+CACLZ,YADK,EACU,EADV,+BAELC,aAFK,EAEW,EAFX,eADV;AAAA,GAD8B,EAM9B,EAN8B,CAAhC;AAQA,MAAMkB,oBAAoB,GAAG7B,WAAW,CAAC,YAAM;AAC7CoB,IAAAA,aAAa,CAACC,KAAD,CAAb;AACAR,IAAAA,YAAY,CAACiB,OAAb,CAAqB,oBAArB,EAA2Cd,IAAI,CAACe,SAAL,CAAeV,KAAf,CAA3C;AACD,GAHuC,EAGrC,CAACA,KAAD,CAHqC,CAAxC;AAKA,sBACE,uDACE,oBAAC,YAAD,qBACE,oBAAC,WAAD,qBACE,oBAAC,SAAD;AAAW,IAAA,OAAO,EAAEK;AAApB,KAAqC,mBAArC,CADF,CADF,eAIE,oBAAC,WAAD,qBACE,oBAAC,SAAD;AAAW,IAAA,OAAO,EAAEC;AAApB,KAAmC,OAAnC,CADF,CAJF,eAOE,oBAAC,WAAD,qBACE,oBAAC,SAAD;AAAW,IAAA,OAAO,EAAEC;AAApB,KAAmC,OAAnC,CADF,CAPF,eAUE,oBAAC,WAAD,qBACE,oBAAC,SAAD;AACE,IAAA,IAAI,EAAET,UAAU,KAAKE,KADvB;AAEE,IAAA,QAAQ,EAAEF,UAAU,KAAKE,KAAf,GAAuB,OAAvB,GAAiCW,SAF7C;AAGE,IAAA,OAAO,EAAEH;AAHX,KAIG,uBAJH,CADF,CAVF,CADF,eAqBE,oBAAC,SAAD,OArBF,eAuBE,oBAAC,qBAAD;AACE,IAAA,KAAK,EAAE;AAAEI,MAAAA,MAAM,EAAE;AAAV,KADT;AAEE,IAAA,kBAAkB,EAAEV;AAFtB,KAGG,UAACW,iBAAD,EAAoBC,kBAApB;AAAA,wBACC,uDACE,oBAAC,iBAAD;AACE,MAAA,EAAE,EAAEzB,YADN;AAEE,MAAA,IAAI,EAAEW,KAAK,CAACX,YAAD,CAFb;AAGE,MAAA,OAAO,EAAC;AAHV,oBAIE,oBAAC,OAAD,qBACE,iCAAMD,IAAN,CADF,CAJF,CADF,eAUE,oBAAC,kBAAD,OAVF,eAYE,oBAAC,iBAAD;AACE,MAAA,EAAE,EAAEE,aADN;AAEE,MAAA,IAAI,EAAEU,KAAK,CAACV,aAAD,CAFb;AAGE,MAAA,OAAO,EAAC;AAHV,oBAIE,oBAAC,OAAD,qBACE,iCAAMF,IAAN,CADF,CAJF,CAZF,CADD;AAAA,GAHH,CAvBF,CADF;AAqDD,CApFD","sourcesContent":["import React, { useCallback, useState } from 'react';\n\nimport {\n  EuiFlexGroup,\n  EuiFlexItem,\n  EuiText,\n  EuiResizableContainer,\n  EuiButton,\n  EuiSpacer,\n} from '../../../../src/components';\nimport { fake } from 'faker';\n\nconst text = (\n  <>\n    <p>{fake('{{lorem.paragraphs}}')}</p>\n    <p>{fake('{{lorem.paragraphs}}')}</p>\n    <p>{fake('{{lorem.paragraphs}}')}</p>\n  </>\n);\n\nconst firstPanelId = 'resizable-panel__1';\nconst secondPanelId = 'resizable-panel__2';\nconst stored = localStorage.getItem('resizableContainer');\nconst storedSizes = stored && JSON.parse(stored);\nconst defaultSizes = storedSizes || {\n  [firstPanelId]: 50,\n  [secondPanelId]: 50,\n};\n\nexport default () => {\n  const [savedSizes, setSavedSizes] = useState(storedSizes);\n  const [sizes, setSizes] = useState(defaultSizes);\n  const onPanelWidthChange = useCallback((newSizes) => {\n    setSizes((prevSizes) => ({\n      ...prevSizes,\n      ...newSizes,\n    }));\n  }, []);\n  const onClickDefault = useCallback(() => setSizes(defaultSizes), []);\n  const onClick30x70 = useCallback(\n    () =>\n      setSizes({\n        [firstPanelId]: 30,\n        [secondPanelId]: 70,\n      }),\n    []\n  );\n  const onClick80x20 = useCallback(\n    () =>\n      setSizes({\n        [firstPanelId]: 80,\n        [secondPanelId]: 20,\n      }),\n    []\n  );\n  const onSaveToLocalStorage = useCallback(() => {\n    setSavedSizes(sizes);\n    localStorage.setItem('resizableContainer', JSON.stringify(sizes));\n  }, [sizes]);\n\n  return (\n    <>\n      <EuiFlexGroup>\n        <EuiFlexItem>\n          <EuiButton onClick={onClickDefault}>{'Reset to defaults'}</EuiButton>\n        </EuiFlexItem>\n        <EuiFlexItem>\n          <EuiButton onClick={onClick30x70}>{'30x70'}</EuiButton>\n        </EuiFlexItem>\n        <EuiFlexItem>\n          <EuiButton onClick={onClick80x20}>{'80x20'}</EuiButton>\n        </EuiFlexItem>\n        <EuiFlexItem>\n          <EuiButton\n            fill={savedSizes === sizes}\n            iconType={savedSizes === sizes ? 'check' : undefined}\n            onClick={onSaveToLocalStorage}>\n            {'Store in localStorage'}\n          </EuiButton>\n        </EuiFlexItem>\n      </EuiFlexGroup>\n\n      <EuiSpacer />\n\n      <EuiResizableContainer\n        style={{ height: '200px' }}\n        onPanelWidthChange={onPanelWidthChange}>\n        {(EuiResizablePanel, EuiResizableButton) => (\n          <>\n            <EuiResizablePanel\n              id={firstPanelId}\n              size={sizes[firstPanelId]}\n              minSize=\"30%\">\n              <EuiText>\n                <div>{text}</div>\n              </EuiText>\n            </EuiResizablePanel>\n\n            <EuiResizableButton />\n\n            <EuiResizablePanel\n              id={secondPanelId}\n              size={sizes[secondPanelId]}\n              minSize=\"200px\">\n              <EuiText>\n                <div>{text}</div>\n              </EuiText>\n            </EuiResizablePanel>\n          </>\n        )}\n      </EuiResizableContainer>\n    </>\n  );\n};\n"]}]}
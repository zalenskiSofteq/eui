A,MACAC,aADA;;AAGnB,MAAIC,KAAK,GAAGC,YAAY,CAACC,OAAb,CAAqBL,WAArB,CAAZ;;AACA,MAAIG,KAAJ,EAAW;AACTA,IAAAA,KAAK,GAAGG,IAAI,CAACC,KAAL,CAAWJ,KAAX,CAAR;AACD,GAFD,MAEO;AACLA,IAAAA,KAAK,GAAGR,UAAR;AACD;;AARkB,oBAU+CN,UAAU,CAC1EC,aAD0E,EAE1Ea,KAF0E,CAVzD;AAAA;AAAA;AAAA,MAUXK,cAVW;AAAA,MAUKC,cAVL;AAAA,MAUsBC,OAVtB;AAAA,MAU+BC,YAV/B;;AAenBhC,EAAAA,SAAS,CAAC,YAAM;AACdyB,IAAAA,YAAY,CAACQ,OAAb,CAAqBZ,WAArB,EAAkCM,IAAI,CAACO,SAAL,CAAeF,YAAf,CAAlC;AACD,GAFQ,EAEN,CAACA,YAAD,CAFM,CAAT;;AAIA,MAAMG,WAAW,GAAG,SAAdA,WAAc,GAAM;AACxBJ,IAAAA,OAAO,CAACK,UAAR;AACD,GAFD;;AAIA,MAAMC,SAAS,GAAG,SAAZA,SAAY,GAAM;AACtBN,IAAAA,OAAO,CAACM,SAAR;AACAd,IAAAA,aAAa,CAAC,EAAD,CAAb;AACD,GAHD;;AAKA,MAAMe,QAAQ,GAAG,SAAXA,QAAW,CAACC,CAAD,EAAO;AACtBhB,IAAAA,aAAa,CAACgB,CAAC,CAACC,MAAF,CAASC,KAAV,CAAb;;AAEA,QAAIT,YAAY,CAACf,eAAb,GAA+B,CAAnC,EAAsC;AACpCc,MAAAA,OAAO,CAACW,aAAR;AACD;AACF,GAND;;AAQA,sBACE,8CACE,oBAAC,cAAD;AAAgB,IAAA,QAAQ,EAAC,SAAzB;AAAmC,IAAA,KAAK,EAAC,MAAzC;AAAgD,IAAA,OAAO,EAAEL;AAAzD,kBADF,eAIE,oBAAC,SAAD,OAJF,eAKE,oBAAC,OAAD;AAAS,IAAA,SAAS,EAAC;AAAnB,kBACE,oBAAC,UAAD;AAAY,IAAA,KAAK,EAAC;AAAlB,kBACE,oBAAC,WAAD,EAAiBR,cAAjB,eACE,oBAAC,WAAD;AACE,IAAA,WAAW,EAAC,kBADd;AAEE,kBAAW,oBAFb;AAGE,IAAA,KAAK,EAAEP,UAHT;AAIE,IAAA,QAAQ,EAAEgB,QAJZ;AAKE,IAAA,KAAK,EAAE;AAAEK,MAAAA,KAAK,EAAE;AAAT;AALT,IADF,CADF,CADF,eAaE,oBAAC,SAAD,OAbF,eAeE,oBAAC,WAAD,EAAiBb,cAAjB,eACE,oBAAC,SAAD;AAAW,IAAA,OAAO,EAAEK;AAApB,kBADF,CAfF,CALF,CADF;AA2BD,CA/DD","sourcesContent":["import React, { useEffect, useState } from 'react';\n\nimport {\n  EuiButton,\n  EuiButtonEmpty,\n  EuiCodeBlock,\n  EuiForm,\n  EuiFormRow,\n  EuiSpacer,\n  EuiTextArea,\n  EuiTourStep,\n  useEuiTour,\n} from '../../../../src/components';\n\nconst demoTourSteps = [\n  {\n    step: 1,\n    title: 'Step 1',\n    content: (\n      <span>\n        <p>Copy and paste this sample query.</p>\n        <EuiSpacer />\n        <EuiCodeBlock language=\"html\" paddingSize=\"s\" isCopyable>\n          {'SELECT email FROM “kibana_sample_data_ecommerce”'}\n        </EuiCodeBlock>\n      </span>\n    ),\n    anchorPosition: 'rightUp',\n  },\n  {\n    step: 2,\n    title: 'Step 2',\n    content: <p>Save your changes.</p>,\n    anchorPosition: 'rightUp',\n  },\n];\n\nconst tourConfig = {\n  currentTourStep: 1,\n  isTourActive: true,\n  tourPopoverWidth: 360,\n  tourSubtitle: 'Demo tour',\n};\n\nconst STORAGE_KEY = 'tourDemo_Managed';\n\nexport default () => {\n  const [queryValue, setQueryValue] = useState('');\n\n  let state = localStorage.getItem(STORAGE_KEY);\n  if (state) {\n    state = JSON.parse(state);\n  } else {\n    state = tourConfig;\n  }\n\n  const [[euiTourStepOne, euiTourStepTwo], actions, reducerState] = useEuiTour(\n    demoTourSteps,\n    state\n  );\n\n  useEffect(() => {\n    localStorage.setItem(STORAGE_KEY, JSON.stringify(reducerState));\n  }, [reducerState]);\n\n  const handleClick = () => {\n    actions.finishTour();\n  };\n\n  const resetTour = () => {\n    actions.resetTour();\n    setQueryValue('');\n  };\n\n  const onChange = (e) => {\n    setQueryValue(e.target.value);\n\n    if (reducerState.currentTourStep < 2) {\n      actions.incrementStep();\n    }\n  };\n\n  return (\n    <div>\n      <EuiButtonEmpty iconType=\"refresh\" flush=\"left\" onClick={resetTour}>\n        Reset tour\n      </EuiButtonEmpty>\n      <EuiSpacer />\n      <EuiForm component=\"form\">\n        <EuiFormRow label=\"Enter an ES SQL query\">\n          <EuiTourStep {...euiTourStepOne}>\n            <EuiTextArea\n              placeholder=\"Placeholder text\"\n              aria-label=\"Enter ES SQL query\"\n              value={queryValue}\n              onChange={onChange}\n              style={{ width: 400 }}\n            />\n          </EuiTourStep>\n        </EuiFormRow>\n\n        <EuiSpacer />\n\n        <EuiTourStep {...euiTourStepTwo}>\n          <EuiButton onClick={handleClick}>Save query</EuiButton>\n        </EuiTourStep>\n      </EuiForm>\n    </div>\n  );\n};\n"]}]}
AACD,eAAD,CAAX;AACD,GAFD;;AAIA,MAAMQ,cAAc,GAAGd,WAAW,CAAC,UAACe,WAAD,EAAiB;AAClDN,IAAAA,UAAU,CAAC,IAAD,CAAV;AACAE,IAAAA,UAAU,CAAC,EAAD,CAAV;AAEAK,IAAAA,YAAY,CAACJ,aAAD,CAAZ,CAJkD,CAMlD;;AACAA,IAAAA,aAAa,GAAGK,UAAU,CAAC,YAAM;AAC/B;AACAR,MAAAA,UAAU,CAAC,KAAD,CAAV;AACAE,MAAAA,UAAU,CACRP,UAAU,CAACc,MAAX,CAAkB,UAACC,MAAD;AAAA,eAChBA,MAAM,CAAChB,KAAP,CAAaiB,WAAb,GAA2BC,QAA3B,CAAoCN,WAAW,CAACK,WAAZ,EAApC,CADgB;AAAA,OAAlB,CADQ,CAAV;AAKD,KARyB,EAQvB,IARuB,CAA1B;AASD,GAhBiC,EAgB/B,EAhB+B,CAAlC;;AAkBA,MAAME,cAAc,GAAG,SAAjBA,cAAiB,CAACP,WAAD,EAAwC;AAAA,QAA1BQ,gBAA0B,uEAAP,EAAO;AAC7D,QAAMC,qBAAqB,GAAGT,WAAW,CAACU,IAAZ,GAAmBL,WAAnB,EAA9B;;AAEA,QAAI,CAACI,qBAAL,EAA4B;AAC1B;AACD;;AAED,QAAME,SAAS,GAAG;AAChBvB,MAAAA,KAAK,EAAEY;AADS,KAAlB,CAP6D,CAW7D;;AACA,QACEQ,gBAAgB,CAACI,SAAjB,CACE,UAACR,MAAD;AAAA,aAAYA,MAAM,CAACS,KAAP,CAAaH,IAAb,GAAoBL,WAApB,OAAsCI,qBAAlD;AAAA,KADF,MAEM,CAAC,CAHT,EAIE;AACA;AACAnB,MAAAA,aAAa,8BAAKD,UAAL,IAAiBsB,SAAjB,GAAb;AACAf,MAAAA,UAAU,8BAAKD,OAAL,IAAcgB,SAAd,GAAV;AACD,KApB4D,CAsB7D;;;AACAnB,IAAAA,WAAW,CAAC,UAACsB,YAAD;AAAA,0CAAsBA,YAAtB,IAAoCH,SAApC;AAAA,KAAD,CAAX;AACD,GAxBD;;AA0BA3B,EAAAA,SAAS,CAAC,YAAM;AACd;AACAe,IAAAA,cAAc,CAAC,EAAD,CAAd;AACD,GAHQ,EAGN,CAACA,cAAD,CAHM,CAAT;AAKA,sBACE,oBAAC,WAAD;AACE,IAAA,WAAW,EAAC,uBADd;AAEE,IAAA,KAAK,MAFP;AAGE,IAAA,OAAO,EAAEJ,OAHX;AAIE,IAAA,eAAe,EAAEJ,eAJnB;AAKE,IAAA,SAAS,EAAEE,SALb;AAME,IAAA,QAAQ,EAAEK,QANZ;AAOE,IAAA,cAAc,EAAEC,cAPlB;AAQE,IAAA,cAAc,EAAEQ;AARlB,IADF;AAYD,CAvED","sourcesContent":["import React, { useState, useEffect, useCallback } from 'react';\n\nimport { EuiComboBox } from '../../../../src/components';\n\nconst allOptionsStatic = [\n  {\n    label: 'Titan',\n    'data-test-subj': 'titanOption',\n  },\n  {\n    label: 'Enceladus',\n  },\n  {\n    label: 'Mimas',\n  },\n  {\n    label: 'Dione',\n  },\n  {\n    label: 'Iapetus',\n  },\n  {\n    label: 'Phoebe',\n  },\n  {\n    label: 'Rhea',\n  },\n  {\n    label:\n      \"Pandora is one of Saturn's moons, named for a Titaness of Greek mythology\",\n  },\n  {\n    label: 'Tethys',\n  },\n  {\n    label: 'Hyperion',\n  },\n];\n\nexport default () => {\n  const [allOptions, setAllOptions] = useState(allOptionsStatic);\n  const [selectedOptions, setSelected] = useState([]);\n  const [isLoading, setLoading] = useState(false);\n  const [options, setOptions] = useState([]);\n  let searchTimeout;\n  const onChange = (selectedOptions) => {\n    setSelected(selectedOptions);\n  };\n\n  const onSearchChange = useCallback((searchValue) => {\n    setLoading(true);\n    setOptions([]);\n\n    clearTimeout(searchTimeout);\n\n    // eslint-disable-next-line react-hooks/exhaustive-deps\n    searchTimeout = setTimeout(() => {\n      // Simulate a remotely-executed search.\n      setLoading(false);\n      setOptions(\n        allOptions.filter((option) =>\n          option.label.toLowerCase().includes(searchValue.toLowerCase())\n        )\n      );\n    }, 1200);\n  }, []);\n\n  const onCreateOption = (searchValue, flattenedOptions = []) => {\n    const normalizedSearchValue = searchValue.trim().toLowerCase();\n\n    if (!normalizedSearchValue) {\n      return;\n    }\n\n    const newOption = {\n      label: searchValue,\n    };\n\n    // Create the option if it doesn't exist.\n    if (\n      flattenedOptions.findIndex(\n        (option) => option.value.trim().toLowerCase() === normalizedSearchValue\n      ) === -1\n    ) {\n      // Simulate creating this option on the server.\n      setAllOptions([...allOptions, newOption]);\n      setOptions([...options, newOption]);\n    }\n\n    // Select the option.\n    setSelected((prevSelected) => [...prevSelected, newOption]);\n  };\n\n  useEffect(() => {\n    // Simulate initial load.\n    onSearchChange('');\n  }, [onSearchChange]);\n\n  return (\n    <EuiComboBox\n      placeholder=\"Search asynchronously\"\n      async\n      options={options}\n      selectedOptions={selectedOptions}\n      isLoading={isLoading}\n      onChange={onChange}\n      onSearchChange={onSearchChange}\n      onCreateOption={onCreateOption}\n    />\n  );\n};\n"]}]}
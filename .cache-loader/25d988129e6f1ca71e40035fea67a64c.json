import moment from 'moment';\n\nimport {\n  EuiDatePicker,\n  EuiFormRow,\n  EuiSpacer,\n} from '../../../../src/components';\n\nexport default () => {\n  const [startDate, setStartDate] = useState(moment());\n\n  const handleChange = (date) => {\n    setStartDate(date);\n  };\n\n  return (\n    <div>\n      <EuiFormRow label=\"Time select on\">\n        <EuiDatePicker\n          showTimeSelect\n          selected={startDate}\n          onChange={handleChange}\n        />\n      </EuiFormRow>\n\n      <EuiSpacer />\n\n      <EuiFormRow label=\"Only time select, 24 hour clock\">\n        <EuiDatePicker\n          showTimeSelect\n          showTimeSelectOnly\n          selected={startDate}\n          onChange={handleChange}\n          dateFormat=\"HH:mm\"\n          timeFormat=\"HH:mm\"\n        />\n      </EuiFormRow>\n\n      <EuiSpacer />\n\n      <EuiFormRow label=\"Inject additional times into the list\">\n        <EuiDatePicker\n          showTimeSelect\n          showTimeSelectOnly\n          selected={startDate}\n          onChange={handleChange}\n          dateFormat=\"hh:mm a\"\n          timeFormat=\"hh:mm a\"\n          injectTimes={[\n            moment().hours(0).minutes(1),\n            moment().hours(0).minutes(5),\n            moment().hours(23).minutes(59),\n          ]}\n        />\n      </EuiFormRow>\n    </div>\n  );\n};\n"]}]}
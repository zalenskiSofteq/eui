,CAFnB;AAGE,IAAA,OAAO,EAAE,mBAAM;AACbD,MAAAA,YAAY;AACZF,MAAAA,UAAU,CAAC,EAAD,CAAV;AACD;AANH,eAnBY,eA4BZ,oBAAC,kBAAD;AACE,IAAA,GAAG,EAAC,UADN;AAEE,IAAA,IAAI,EAAEG,WAAW,CAAC,GAAD,CAFnB;AAGE,IAAA,OAAO,EAAE,mBAAM;AACbD,MAAAA,YAAY;AACZF,MAAAA,UAAU,CAAC,GAAD,CAAV;AACD;AANH,gBA5BY,CAAd;AAuCA,sBACE,oBAAC,UAAD;AACE,IAAA,EAAE,EAAC,aADL;AAEE,IAAA,MAAM,EAAEK,MAFV;AAGE,IAAA,MAAM,EAAER,aAHV;AAIE,IAAA,YAAY,EAAEK,YAJhB;AAKE,IAAA,gBAAgB,EAAC,MALnB;AAME,IAAA,cAAc,EAAC;AANjB,kBAOE,oBAAC,mBAAD;AAAqB,IAAA,IAAI,EAAC,GAA1B;AAA8B,IAAA,KAAK,EAAEI;AAArC,IAPF,CADF;AAWD,CA5ED","sourcesContent":["import React, { useState } from 'react';\n\nimport {\n  EuiButtonEmpty,\n  EuiContextMenuPanel,\n  EuiContextMenuItem,\n  EuiPopover,\n} from '../../../../src/components';\n\nexport default () => {\n  const [isPopoverOpen, setPopover] = useState(false);\n  const [rowSize, setRowSize] = useState(50);\n\n  const onButtonClick = () => {\n    setPopover(!isPopoverOpen);\n  };\n\n  const closePopover = () => {\n    setPopover(false);\n  };\n\n  const getIconType = (size) => {\n    return size === rowSize ? 'check' : 'empty';\n  };\n\n  const button = (\n    <EuiButtonEmpty\n      size=\"s\"\n      iconType=\"arrowDown\"\n      iconSide=\"right\"\n      onClick={onButtonClick}>\n      Rows per page: {rowSize}\n    </EuiButtonEmpty>\n  );\n\n  const items = [\n    <EuiContextMenuItem\n      key=\"10 rows\"\n      icon={getIconType(10)}\n      onClick={() => {\n        closePopover();\n        setRowSize(10);\n      }}>\n      10 rows\n    </EuiContextMenuItem>,\n    <EuiContextMenuItem\n      key=\"20 rows\"\n      icon={getIconType(20)}\n      onClick={() => {\n        closePopover();\n        setRowSize(20);\n      }}>\n      20 rows\n    </EuiContextMenuItem>,\n    <EuiContextMenuItem\n      key=\"50 rows\"\n      icon={getIconType(50)}\n      onClick={() => {\n        closePopover();\n        setRowSize(50);\n      }}>\n      50 rows\n    </EuiContextMenuItem>,\n    <EuiContextMenuItem\n      key=\"100 rows\"\n      icon={getIconType(100)}\n      onClick={() => {\n        closePopover();\n        setRowSize(100);\n      }}>\n      100 rows\n    </EuiContextMenuItem>,\n  ];\n\n  return (\n    <EuiPopover\n      id=\"singlePanel\"\n      button={button}\n      isOpen={isPopoverOpen}\n      closePopover={closePopover}\n      panelPaddingSize=\"none\"\n      anchorPosition=\"downLeft\">\n      <EuiContextMenuPanel size=\"s\" items={items} />\n    </EuiPopover>\n  );\n};\n"]}]}
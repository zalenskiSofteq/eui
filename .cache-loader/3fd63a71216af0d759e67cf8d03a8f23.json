ame=\"custom\" paddingSize=\"m\">\n                <EuiFlexGroup>\n                  <EuiFlexItem grow={false}>\n                    <div {...provided.dragHandleProps} aria-label=\"Drag Handle\">\n                      <EuiIcon type=\"grab\" />\n                    </div>\n                  </EuiFlexItem>\n                  <EuiFlexItem>{content}</EuiFlexItem>\n                </EuiFlexGroup>\n              </EuiPanel>\n            )}\n          </EuiDraggable>\n        ))}\n      </EuiDroppable>\n    </EuiDragDropContext>\n  );\n};\n"]}]}
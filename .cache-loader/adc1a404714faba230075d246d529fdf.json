0B,MAAA,WAAW,EAAC;AAAtC,oBACE;AAAK,MAAA,QAAQ,EAAE,CAAf;AAAkB,MAAA,SAAS,EAAC;AAA5B,oBACE,oBAAC,OAAD;AAAS,MAAA,IAAI,EAAC,GAAd;AAAkB,MAAA,KAAK,EAAE;AAAEC,QAAAA,KAAK,EAAE,MAAT;AAAiBD,QAAAA,OAAO,EAAE;AAA1B;AAAzB,oBACE,8QADF,CADF,CADF,CAvBJ;AAoCE,IAAA,OAAO;AApCT,IApEF,eA8GE,oBAAC,SAAD,OA9GF,eA+GE,oBAAC,qBAAD;AACE,IAAA,IAAI,EAAC,gBADP;AAEE,IAAA,WAAW,eACT,uDACE,2LADF,eAME,6CACa,GADb,eAEE,oBAAC,OAAD;AAAS,MAAA,QAAQ,EAAC;AAAlB,yCAFF,EAEqE,GAFrE,iCAGgB,oBAAC,OAAD;AAAS,MAAA,QAAQ,EAAC;AAAlB,yBAHhB,oCAIc,oBAAC,OAAD;AAAS,MAAA,QAAQ,EAAC;AAAlB,cAJd,iBANF,eAYE,6CACE,8CADF,eAEE,6CACE,oBAAC,OAAD;AAAS,MAAA,QAAQ,EAAC;AAAlB,iCADF,CAFF,CAZF,CAHJ;AAuBE,IAAA,OAAO,eACL;AAAK,MAAA,KAAK,EAAE;AAAEF,QAAAA,MAAM,EAAE;AAAV;AAAZ,oBACE,oBAAC,YAAD;AACE,MAAA,SAAS,EAAC,gBADZ;AAEE,MAAA,UAAU,EAAC,GAFb;AAGE,MAAA,UAAU,EAAE;AAHd,oBAIE,oBAAC,WAAD,qBACE,oBAAC,QAAD;AAAU,MAAA,SAAS,EAAC,aAApB;AAAkC,MAAA,KAAK,EAAC,SAAxC;AAAkD,MAAA,QAAQ,EAAC;AAA3D,oBACE,oBAAC,OAAD;AAAS,MAAA,IAAI,EAAC;AAAd,oBACE,8QADF,CADF,CADF,CAJF,eAiBE,oBAAC,WAAD,qBACE,oBAAC,QAAD;AAAU,MAAA,SAAS,EAAC,aAApB;AAAkC,MAAA,KAAK,EAAC,SAAxC;AAAkD,MAAA,QAAQ,EAAC;AAA3D,oBACE,oBAAC,OAAD;AAAS,MAAA,IAAI,EAAC;AAAd,oBACE,8QADF,CADF,CADF,CAjBF,CADF,CAxBJ;AA0DE,IAAA,OAAO;AA1DT,IA/GF,CADa;AAAA,CAAf","sourcesContent":["import React from 'react';\n\nimport {\n  EuiText,\n  EuiCode,\n  EuiSpacer,\n  EuiPanel,\n  EuiFlexItem,\n  EuiFlexGroup,\n} from '../../../../src/components';\nimport { UtilityClassesSection } from './utility_classes_section';\n\nexport default () => (\n  <>\n    <UtilityClassesSection\n      code=\"eui-yScroll\"\n      description={\n        <>\n          <p>\n            Quick utility for adding vertical scrolling to a container. Requires\n            the wrapping element to control the height and to have\n            <EuiCode language=\"sass\">overflow-y: hidden;</EuiCode> applied.\n          </p>\n          <p>\n            If you would like the content to fade at the top and bottom, use the\n            variant <EuiCode>eui-yScrollWithShadows</EuiCode>.\n          </p>\n          <dl>\n            <dt>Sass mixins</dt>\n            <dd>\n              <EuiCode language=\"scss\">@include euiYScroll;</EuiCode>\n            </dd>\n            <dd>\n              <EuiCode language=\"scss\">@include euiYScrollWithShadows;</EuiCode>\n            </dd>\n          </dl>\n        </>\n      }\n      example={\n        <EuiPanel\n          color=\"warning\"\n          paddingSize=\"none\"\n          style={{\n            height: 180,\n            overflowY: 'hidden',\n          }}>\n          <EuiText\n            tabIndex={0}\n            className=\"eui-yScrollWithShadows\"\n            size=\"s\"\n            style={{ padding: 16 }}>\n            <p>\n              Orbiting this at a distance of roughly ninety-two million miles is\n              an utterly insignificant little blue green planet whose\n              ape-descended life forms are so amazingly primitive that they\n              still think digital watches are a pretty neat idea.\n            </p>\n            <p>\n              Orbiting this at a distance of roughly ninety-two million miles is\n              an utterly insignificant little blue green planet whose\n              ape-descended life forms are so amazingly primitive that they\n              still think digital watches are a pretty neat idea.\n            </p>\n            <p>\n              Orbiting this at a distance of roughly ninety-two million miles is\n              an utterly insignificant little blue green planet whose\n              ape-descended life forms are so amazingly primitive that they\n              still think digital watches are a pretty neat idea.\n            </p>\n          </EuiText>\n        </EuiPanel>\n      }\n      snippet={`<BodyContent\n  style={{ height: 200, overflowY: 'hidden' }}>\n  <BodyScroll\n    className=\"eui-yScrollWithShadows\"\n    tabIndex={0}\n  />\n</BodyContent>`}\n    />\n    <EuiSpacer />\n    <UtilityClassesSection\n      code=\"eui-xScroll\"\n      description={\n        <>\n          <p>Quick utility for adding horizontal scrolling to a container.</p>\n          <p>\n            If you would like the content to fade at the left and right, use the\n            variant <EuiCode>eui-xScrollWithShadows</EuiCode>. It is recommended\n            to add padding to the sides of the inner content so the mask\n            doesn&apos;t overlay it.\n          </p>\n          <dl>\n            <dt>Sass mixins</dt>\n            <dd>\n              <EuiCode language=\"scss\">@include euiXScroll;</EuiCode>\n            </dd>\n            <dd>\n              <EuiCode language=\"scss\">@include euiXScrollWithShadows;</EuiCode>\n            </dd>\n          </dl>\n        </>\n      }\n      example={\n        <EuiPanel color=\"warning\" paddingSize=\"none\">\n          <div tabIndex={0} className=\"eui-xScrollWithShadows\">\n            <EuiText size=\"s\" style={{ width: '150%', padding: 16 }}>\n              <p>\n                Orbiting this at a distance of roughly ninety-two million miles\n                is an utterly insignificant little blue green planet whose\n                ape-descended life forms are so amazingly primitive that they\n                still think digital watches are a pretty neat idea.\n              </p>\n            </EuiText>\n          </div>\n        </EuiPanel>\n      }\n      snippet={`<BodyScroll\n  className=\"eui-xScrollWithShadows\"\n  tabIndex={0}>\n  <BodyContent style={{ width: '150%', padding: 16 }} />\n</BodyScroll>`}\n    />\n    <EuiSpacer />\n    <UtilityClassesSection\n      code=\"eui-fullHeight\"\n      description={\n        <>\n          <p>\n            Quick utility for expanding the height of the element to its parents\n            dimensions. Use it to stretch each nested element until the one that\n            applies scroll.\n          </p>\n          <p>\n            It applies{' '}\n            <EuiCode language=\"sass\">height: 100%; overflow: hidden;</EuiCode>{' '}\n            but also adds <EuiCode language=\"sass\">flex: 1 1 auto;</EuiCode> for\n            uses within <EuiCode language=\"sass\">flex</EuiCode> containers.\n          </p>\n          <dl>\n            <dt>Sass mixins</dt>\n            <dd>\n              <EuiCode language=\"scss\">@include euiFullHeight;</EuiCode>\n            </dd>\n          </dl>\n        </>\n      }\n      example={\n        <div style={{ height: 180 }}>\n          <EuiFlexGroup\n            className=\"eui-fullHeight\"\n            gutterSize=\"s\"\n            responsive={false}>\n            <EuiFlexItem>\n              <EuiPanel className=\"eui-yScroll\" color=\"warning\" tabIndex=\"0\">\n                <EuiText size=\"s\">\n                  <p>\n                    Orbiting this at a distance of roughly ninety-two million\n                    miles is an utterly insignificant little blue green planet\n                    whose ape-descended life forms are so amazingly primitive\n                    that they still think digital watches are a pretty neat\n                    idea.\n                  </p>\n                </EuiText>\n              </EuiPanel>\n            </EuiFlexItem>\n            <EuiFlexItem>\n              <EuiPanel className=\"eui-yScroll\" color=\"warning\" tabIndex=\"0\">\n                <EuiText size=\"s\">\n                  <p>\n                    Orbiting this at a distance of roughly ninety-two million\n                    miles is an utterly insignificant little blue green planet\n                    whose ape-descended life forms are so amazingly primitive\n                    that they still think digital watches are a pretty neat\n                    idea.\n                  </p>\n                </EuiText>\n              </EuiPanel>\n            </EuiFlexItem>\n          </EuiFlexGroup>\n        </div>\n      }\n      snippet={`<BodyContent style={{ height: 180 }}>\n  <EuiFlexGroup\n    className=\"eui-fullHeight\" responsive={false}>\n    <EuiFlexItem>\n      <BodyScroll\n        className=\"eui-yScroll\" tabIndex=\"0\"/>\n    </EuiFlexItem>\n    <EuiFlexItem>\n      <BodyScroll\n        className=\"eui-yScroll\" tabIndex=\"0\"/>\n    </EuiFlexItem>\n  </EuiFlexGroup>\n</BodyContent>`}\n    />\n  </>\n);\n"]}]}
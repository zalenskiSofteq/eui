playToggles } from '../form_controls/display_toggles';\n\nexport default () => {\n  const [startDate, setStartDate] = useState(null);\n\n  const handleChange = (date) => {\n    setStartDate(date);\n  };\n\n  const errors = [\n    \"Here's an example of an error\",\n    'You might have more than one error, so pass an array.',\n  ];\n\n  return (\n    /* DisplayToggles wrapper for Docs only */\n    <div>\n      <DisplayToggles canCompressed={false}>\n        <EuiDatePicker\n          showTimeSelect\n          selected={startDate}\n          onChange={handleChange}\n          placeholder=\"Placeholder text\"\n        />\n      </DisplayToggles>\n\n      <EuiSpacer size=\"l\" />\n\n      <EuiDatePicker\n        showTimeSelect\n        selected={startDate}\n        onChange={handleChange}\n        onClear={() => handleChange(null)}\n        placeholder=\"Clearable\"\n      />\n\n      <EuiSpacer size=\"m\" />\n\n      <EuiFormRow label=\"Form row validation\" isInvalid error={errors}>\n        <EuiDatePicker\n          showTimeSelect\n          isInvalid\n          selected={startDate}\n          onChange={handleChange}\n          placeholder=\"Example of an error\"\n        />\n      </EuiFormRow>\n    </div>\n  );\n};\n"]}]}
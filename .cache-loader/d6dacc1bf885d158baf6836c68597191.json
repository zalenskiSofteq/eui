esContent":["import React, { useState } from 'react';\n\nimport moment from 'moment';\n\nimport {\n  EuiDatePicker,\n  EuiFormRow,\n  EuiSpacer,\n} from '../../../../src/components';\n\nexport default () => {\n  const [startDate, setStartDate] = useState(moment());\n\n  const handleChange = (date) => {\n    setStartDate(date);\n  };\n\n  return (\n    <div>\n      <EuiFormRow label=\"US with fractional seconds\">\n        <EuiDatePicker\n          selected={startDate}\n          showTimeSelect\n          onChange={handleChange}\n          dateFormat=\"YYYY-MM-DD hh:mm:ss:SSS A\"\n        />\n      </EuiFormRow>\n\n      <EuiSpacer size=\"m\" />\n\n      <EuiFormRow label=\"China\">\n        <EuiDatePicker\n          selected={startDate}\n          showTimeSelect\n          onChange={handleChange}\n          dateFormat=\"YYYY-MM-DD hh:mm A\"\n          locale=\"zh-cn\"\n        />\n      </EuiFormRow>\n\n      <EuiSpacer size=\"m\" />\n\n      <EuiFormRow label=\"Korea\">\n        <EuiDatePicker\n          selected={startDate}\n          showTimeSelect\n          onChange={handleChange}\n          locale=\"ko\"\n          dateFormat=\"YYYY-MM-DD hh:mm A\"\n        />\n      </EuiFormRow>\n\n      <EuiSpacer size=\"m\" />\n\n      <EuiFormRow label=\"Germany on 24 hour clock\">\n        <EuiDatePicker\n          selected={startDate}\n          showTimeSelect\n          onChange={handleChange}\n          dateFormat=\"DD-MM-YYYY HH:mm\"\n          timeFormat=\"HH:mm\"\n          locale=\"de-de\"\n        />\n      </EuiFormRow>\n    </div>\n  );\n};\n"]}]}
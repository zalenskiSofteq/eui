,oBAAC,SAAD;AAAW,IAAA,IAAI,EAAC;AAAhB,IAXF,CADF;AAeD,CArCM","sourcesContent":["import React, { useState } from 'react';\n\nimport {\n  EuiSwitch,\n  EuiDescriptionList,\n  EuiSpacer,\n} from '../../../../src/components';\n\nimport { EuiWindowEvent } from '../../../../src/services';\n\nexport const MousePosition = () => {\n  const [tracking, setTracking] = useState(false);\n  const [coordinates, setCoordinates] = useState({});\n\n  const onSwitchChange = () => {\n    setTracking(!tracking);\n  };\n\n  const onMouseMove = ({ clientX, clientY }) => {\n    setCoordinates({ clientX, clientY });\n  };\n\n  const listItems = [\n    {\n      title: 'Position X',\n      description: coordinates.clientX || '??',\n    },\n    {\n      title: 'Position Y',\n      description: coordinates.clientY || '??',\n    },\n  ];\n  return (\n    <div>\n      <EuiSwitch\n        label=\"Track mouse position\"\n        checked={tracking}\n        onChange={onSwitchChange}\n      />\n      {tracking ? (\n        <EuiWindowEvent event=\"mousemove\" handler={onMouseMove} />\n      ) : null}\n      <EuiSpacer size=\"l\" />\n      <EuiDescriptionList listItems={listItems} />\n      <EuiSpacer size=\"xxl\" />\n    </div>\n  );\n};\n"]}]}
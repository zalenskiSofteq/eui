AAlB;AAAyB,MAAA,IAAI,EAAE;AAA/B,OACGD,KADH,CADF;AAKD,GAND,CA/FJ,CADF,CAhBF,CAHF,CADF;AAgID,CAnKM;AAqKPvC,cAAc,CAACyC,SAAf,GAA2B;AACzBxC,EAAAA,aAAa,EAAEV,SAAS,CAACmD,IADA;AAEzBxC,EAAAA,WAAW,EAAEX,SAAS,CAACmD,IAFE;AAGzBvC,EAAAA,WAAW,EAAEZ,SAAS,CAACmD,IAHE;AAIzBtC,EAAAA,UAAU,EAAEb,SAAS,CAACmD,IAJG;AAKzBrC,EAAAA,aAAa,EAAEd,SAAS,CAACmD,IALA;AAMzBpC,EAAAA,YAAY,EAAEf,SAAS,CAACmD,IANC;AAOzBnC,EAAAA,UAAU,EAAEhB,SAAS,CAACmD,IAPG;AAQzBlC,EAAAA,SAAS,EAAEjB,SAAS,CAACmD,IARI;AASzBjC,EAAAA,UAAU,EAAElB,SAAS,CAACmD,IATG;AAUzB/B,EAAAA,MAAM,EAAEpB,SAAS,CAACoD,OAAV,CAAkBpD,SAAS,CAACqD,IAA5B,CAViB;AAWzB;AACAhC,EAAAA,UAAU,EAAErB,SAAS,CAACsD,KAAV,CAAgB,CAAC,IAAD,EAAO,GAAP,EAAY,GAAZ,EAAiB,GAAjB,EAAsB,IAAtB,EAA4B,KAA5B,CAAhB;AAZa,CAA3B;AAeA7C,cAAc,CAAC8C,YAAf,GAA8B;AAC5B7C,EAAAA,aAAa,EAAE,KADa;AAE5BC,EAAAA,WAAW,EAAE,IAFe;AAG5BC,EAAAA,WAAW,EAAE,IAHe;AAI5BC,EAAAA,UAAU,EAAE,IAJgB;AAK5BC,EAAAA,aAAa,EAAE,IALa;AAM5BC,EAAAA,YAAY,EAAE,IANc;AAO5BG,EAAAA,UAAU,EAAE,IAPgB;AAQ5BF,EAAAA,UAAU,EAAE,KARgB;AAS5BC,EAAAA,SAAS,EAAE,KATiB;AAU5BI,EAAAA,UAAU,EAAE;AAVgB,CAA9B","sourcesContent":["import React, { cloneElement, useState, Fragment } from 'react';\nimport PropTypes from 'prop-types';\n\nimport {\n  EuiFlexGroup,\n  EuiSwitch,\n  EuiFlexItem,\n  EuiToolTip,\n  EuiIcon,\n  EuiButtonEmpty,\n  EuiPopover,\n  EuiSpacer,\n} from '../../../../src/components';\n\nexport const DisplayToggles = ({\n  canIsDisabled,\n  canDisabled,\n  canReadOnly,\n  canLoading,\n  canCompressed,\n  canFullWidth,\n  canPrepend,\n  canAppend,\n  canInvalid,\n  children,\n  extras,\n  spacerSize,\n}) => {\n  const [disabled, setDisabled] = useState(false);\n  const [readOnly, setReadOnly] = useState(false);\n  const [loading, setLoading] = useState(false);\n  const [compressed, setCompressed] = useState(false);\n  const [fullWidth, setFullWidth] = useState(false);\n  const [prepend, setPrepend] = useState(false);\n  const [append, setAppend] = useState(false);\n  const [isPopoverOpen, setIsPopoverOpen] = useState(false);\n  const [invalid, setInvalid] = useState(false);\n\n  const canProps = {};\n  if (canDisabled) canProps.disabled = disabled;\n  if (canIsDisabled) canProps.isDisabled = disabled;\n  if (canReadOnly) canProps.readOnly = readOnly;\n  if (canLoading) canProps.isLoading = loading;\n  if (canFullWidth) canProps.fullWidth = fullWidth;\n  if (canCompressed) canProps.compressed = compressed;\n  if (canPrepend && prepend) canProps.prepend = 'Prepend';\n  if (canAppend && append) canProps.append = 'Append';\n  if (canInvalid) canProps.isInvalid = invalid;\n\n  return (\n    <Fragment>\n      {cloneElement(children, canProps)}\n      <EuiSpacer size={spacerSize} />\n      <EuiPopover\n        panelPaddingSize=\"s\"\n        isOpen={isPopoverOpen}\n        closePopover={() => {\n          setIsPopoverOpen(false);\n        }}\n        button={\n          <EuiButtonEmpty\n            iconType=\"controlsHorizontal\"\n            size=\"xs\"\n            onClick={() => {\n              setIsPopoverOpen(!isPopoverOpen);\n            }}>\n            Display toggles\n          </EuiButtonEmpty>\n        }>\n        <div>\n          <EuiFlexGroup\n            wrap={true}\n            direction=\"column\"\n            gutterSize=\"s\"\n            responsive={false}>\n            {(canDisabled || canIsDisabled) && (\n              <EuiFlexItem grow={false}>\n                <EuiSwitch\n                  compressed\n                  label={'disabled'}\n                  checked={disabled}\n                  onChange={(e) => setDisabled(e.target.checked)}\n                />\n              </EuiFlexItem>\n            )}\n            {canReadOnly && (\n              <EuiFlexItem grow={false}>\n                <EuiSwitch\n                  compressed\n                  label={'readOnly'}\n                  checked={readOnly}\n                  onChange={(e) => setReadOnly(e.target.checked)}\n                />\n              </EuiFlexItem>\n            )}\n            {canLoading && (\n              <EuiFlexItem grow={false}>\n                <EuiSwitch\n                  compressed\n                  label={'loading'}\n                  checked={loading}\n                  onChange={(e) => setLoading(e.target.checked)}\n                />\n              </EuiFlexItem>\n            )}\n            {canInvalid && (\n              <EuiFlexItem grow={false}>\n                <EuiSwitch\n                  compressed\n                  label={'invalid'}\n                  checked={invalid}\n                  onChange={(e) => setInvalid(e.target.checked)}\n                />\n              </EuiFlexItem>\n            )}\n            {canFullWidth && (\n              <EuiFlexItem grow={false}>\n                <EuiSwitch\n                  compressed\n                  label={'fullWidth'}\n                  checked={fullWidth}\n                  onChange={(e) => setFullWidth(e.target.checked)}\n                />\n              </EuiFlexItem>\n            )}\n            {canCompressed && (\n              <EuiFlexItem grow={false}>\n                <EuiSwitch\n                  compressed\n                  label={\n                    <span>\n                      compressed{' '}\n                      <EuiToolTip content=\"Compressed usages are very specific. Click to view full compressed documentation\">\n                        <a href=\"/#/forms/compressed-forms\">\n                          <EuiIcon type=\"help\" />\n                        </a>\n                      </EuiToolTip>\n                    </span>\n                  }\n                  checked={compressed}\n                  onChange={(e) => setCompressed(e.target.checked)}\n                />\n              </EuiFlexItem>\n            )}\n            {canPrepend && (\n              <EuiFlexItem grow={false}>\n                <EuiSwitch\n                  compressed\n                  label={'prepend'}\n                  checked={prepend}\n                  onChange={(e) => setPrepend(e.target.checked)}\n                />\n              </EuiFlexItem>\n            )}\n            {canAppend && (\n              <EuiFlexItem grow={false}>\n                <EuiSwitch\n                  compressed\n                  label={'append'}\n                  checked={append}\n                  onChange={(e) => setAppend(e.target.checked)}\n                />\n              </EuiFlexItem>\n            )}\n            {extras &&\n              extras.map((extra, index) => {\n                return (\n                  <EuiFlexItem key={index} grow={false}>\n                    {extra}\n                  </EuiFlexItem>\n                );\n              })}\n          </EuiFlexGroup>\n        </div>\n      </EuiPopover>\n    </Fragment>\n  );\n};\n\nDisplayToggles.propTypes = {\n  canIsDisabled: PropTypes.bool,\n  canDisabled: PropTypes.bool,\n  canReadOnly: PropTypes.bool,\n  canLoading: PropTypes.bool,\n  canCompressed: PropTypes.bool,\n  canFullWidth: PropTypes.bool,\n  canPrepend: PropTypes.bool,\n  canAppend: PropTypes.bool,\n  canInvalid: PropTypes.bool,\n  extras: PropTypes.arrayOf(PropTypes.node),\n  // Manually building the spacer array to avoid having to import Spacer into codesandbox\n  spacerSize: PropTypes.oneOf(['xs', 's', 'm', 'l', 'xl', 'xxl']),\n};\n\nDisplayToggles.defaultProps = {\n  canIsDisabled: false,\n  canDisabled: true,\n  canReadOnly: true,\n  canLoading: true,\n  canCompressed: true,\n  canFullWidth: true,\n  canInvalid: true,\n  canPrepend: false,\n  canAppend: false,\n  spacerSize: 'l',\n};\n"]}]}
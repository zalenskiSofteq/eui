Ac,IAAA,SAAS,EAAC,4BAAxB;AAAqD,IAAA,KAAK,EAAE;AAA5D,kBACE,oBAAC,gBAAD;AACE,IAAA,WAAW,EAAE;AACXE,MAAAA,KAAK,EAAE,QADI;AAEXC,MAAAA,QAAQ,EAAE,OAFC;AAGXC,MAAAA,QAAQ,EAAE;AAHC,KADf;AAME,IAAA,IAAI,EAAC,GANP;AAOE,IAAA,KAAK,EAAC;AAPR,IADF,eAUE,oBAAC,gBAAD;AACE,IAAA,WAAW,EAAE;AACXF,MAAAA,KAAK,EAAE,QADI;AAEXC,MAAAA,QAAQ,EAAE,OAFC;AAGXC,MAAAA,QAAQ,EAAE;AAHC,KADf;AAME,IAAA,IAAI,EAAC,GANP;AAOE,IAAA,KAAK,EAAC;AAPR,IAVF,CADF,CAPF,EA6BGR,KAAK,CAACS,KAAN,KAAgB,EAAhB,gBACC,oBAAC,gBAAD;AAAkB,IAAA,WAAW,EAAC;AAA9B,kBACE,oBAAC,YAAD;AAAc,IAAA,SAAS,EAAC,YAAxB;AAAqC,IAAA,UAAU,EAAC;AAAhD,kBACE,oBAAC,WAAD;AAAa,IAAA,IAAI,EAAE;AAAnB,kBACE,oBAAC,SAAD;AAAW,IAAA,IAAI,EAAC,GAAhB;AAAoB,IAAA,IAAI;AAAxB,YADF,CADF,CADF,CADD,GAUGC,SAvCN,CARF,CADF;AAoDD,CA1ED","sourcesContent":["import React, { useState } from 'react';\n\nimport {\n  EuiButton,\n  EuiButtonEmpty,\n  EuiFlexGroup,\n  EuiFlexItem,\n  EuiIcon,\n  EuiListGroup,\n  EuiListGroupItem,\n  EuiPopover,\n  EuiPopoverFooter,\n  EuiPopoverTitle,\n  EuiText,\n} from '../../../../src/components';\n\nexport default (props) => {\n  const [isPopoverOpen, setPopover] = useState(false);\n\n  const togglePopover = () => {\n    setPopover(!isPopoverOpen);\n  };\n\n  const closePopover = () => {\n    setPopover(false);\n  };\n\n  const hashtagButton = (\n    <EuiButtonEmpty\n      onClick={togglePopover}\n      size=\"xs\"\n      iconType=\"arrowDown\"\n      aria-label=\"Saved Queries popover\"\n      iconSide=\"right\">\n      <EuiIcon type=\"number\" />\n    </EuiButtonEmpty>\n  );\n\n  return (\n    <EuiPopover\n      id=\"popover\"\n      button={hashtagButton}\n      isOpen={isPopoverOpen}\n      anchorPosition=\"downLeft\"\n      panelPaddingSize=\"none\"\n      closePopover={closePopover}>\n      <EuiPopoverTitle paddingSize=\"s\">SAVED QUERIES</EuiPopoverTitle>\n      <div>\n        <EuiText\n          size=\"s\"\n          color=\"subdued\"\n          className=\"savedQueryManagement__text\">\n          <p>Save query text and filters that you want to use again.</p>\n        </EuiText>\n        <div className=\"savedQueryManagement__listWrapper\">\n          <EuiListGroup className=\"savedQueryManagement__list\" flush={true}>\n            <EuiListGroupItem\n              extraAction={{\n                color: 'danger',\n                iconType: 'trash',\n                iconSize: 's',\n              }}\n              href=\"#\"\n              label=\"Popular shoes in America\"\n            />\n            <EuiListGroupItem\n              extraAction={{\n                color: 'danger',\n                iconType: 'trash',\n                iconSize: 's',\n              }}\n              href=\"#\"\n              label=\"Popular shirts in Canada\"\n            />\n          </EuiListGroup>\n        </div>\n        {props.value !== '' ? (\n          <EuiPopoverFooter paddingSize=\"s\">\n            <EuiFlexGroup direction=\"rowReverse\" alignItems=\"center\">\n              <EuiFlexItem grow={false}>\n                <EuiButton size=\"s\" fill>\n                  Save\n                </EuiButton>\n              </EuiFlexItem>\n            </EuiFlexGroup>\n          </EuiPopoverFooter>\n        ) : undefined}\n      </div>\n    </EuiPopover>\n  );\n};\n"]}]}
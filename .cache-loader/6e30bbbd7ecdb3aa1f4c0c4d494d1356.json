AAD;AACE,IAAA,cAAc,MADhB;AAEE,IAAA,QAAQ,EAAEF,UAFZ;AAGE,IAAA,QAAQ,EAAEC,aAHZ;AAIE,IAAA,OAAO,EAAEP,MAAM,GAAGmB,KAAT,CAAe,EAAf,EAAmBC,OAAnB,CAA2B,CAA3B,CAJX;AAKE,IAAA,OAAO,EAAEpB,MAAM,GAAGmB,KAAT,CAAe,EAAf,EAAmBC,OAAnB,CAA2B,EAA3B;AALX,IADF,CAbF,eAuBE,oBAAC,SAAD,OAvBF,eAyBE,oBAAC,UAAD;AAAY,IAAA,KAAK,EAAC;AAAlB,kBACE,oBAAC,aAAD;AACE,IAAA,cAAc,MADhB;AAEE,IAAA,QAAQ,EAAEX,UAFZ;AAGE,IAAA,QAAQ,EAAEC,aAHZ;AAIE,IAAA,YAAY,EAAE,CAACV,MAAM,EAAP,EAAWA,MAAM,GAAGQ,GAAT,CAAa,CAAb,EAAgB,MAAhB,CAAX;AAJhB,IADF,CAzBF,eAkCE,oBAAC,SAAD,OAlCF,eAoCE,oBAAC,UAAD;AAAY,IAAA,KAAK,EAAC;AAAlB,kBACE,oBAAC,aAAD;AACE,IAAA,cAAc,MADhB;AAEE,IAAA,QAAQ,EAAEG,UAFZ;AAGE,IAAA,QAAQ,EAAEC,aAHZ;AAIE,IAAA,YAAY,EAAE,CACZZ,MAAM,GAAGmB,KAAT,CAAe,CAAf,EAAkBC,OAAlB,CAA0B,CAA1B,CADY,EAEZpB,MAAM,GAAGmB,KAAT,CAAe,EAAf,EAAmBC,OAAnB,CAA2B,CAA3B,CAFY;AAJhB,IADF,CApCF,eAgDE,oBAAC,SAAD,OAhDF,eAkDE,oBAAC,UAAD;AAAY,IAAA,KAAK,EAAC;AAAlB,kBACE,oBAAC,aAAD;AACE,IAAA,cAAc,MADhB;AAEE,IAAA,QAAQ,EAAEP,UAFZ;AAGE,IAAA,QAAQ,EAAEC,aAHZ;AAIE,IAAA,UAAU,EAAEC;AAJd,IADF,CAlDF,CADF;AA6DD,CAzED","sourcesContent":["import React, { useState } from 'react';\n\nimport moment from 'moment';\n\nimport {\n  EuiDatePicker,\n  EuiFormRow,\n  EuiSpacer,\n} from '../../../../src/components';\n\nexport default () => {\n  const [startDate, setStartDate] = useState(moment());\n  const [startDate2, setStartDate2] = useState(moment());\n  const [startDate3, setStartDate3] = useState(moment().add(1, 'days'));\n  const [startDate4, setStartDate4] = useState(moment().add(1, 'days'));\n  const [startDate5, setStartDate5] = useState(moment());\n\n  const isWeekday = (date) => {\n    const day = date.day();\n    return day !== 0 && day !== 6;\n  };\n\n  return (\n    <div>\n      <EuiFormRow label=\"Only allow a certain range of dates\">\n        <EuiDatePicker\n          showTimeSelect\n          selected={startDate}\n          onChange={setStartDate}\n          minDate={moment().subtract(2, 'days')}\n          maxDate={moment().add(5, 'days')}\n        />\n      </EuiFormRow>\n\n      <EuiSpacer />\n\n      <EuiFormRow label=\"Only allow a certain range of times\">\n        <EuiDatePicker\n          showTimeSelect\n          selected={startDate2}\n          onChange={setStartDate2}\n          minTime={moment().hours(17).minutes(0)}\n          maxTime={moment().hours(20).minutes(30)}\n        />\n      </EuiFormRow>\n\n      <EuiSpacer />\n\n      <EuiFormRow label=\"Exclude yesterday and today\">\n        <EuiDatePicker\n          showTimeSelect\n          selected={startDate3}\n          onChange={setStartDate3}\n          excludeDates={[moment(), moment().add(1, 'days')]}\n        />\n      </EuiFormRow>\n\n      <EuiSpacer />\n\n      <EuiFormRow label=\"Exclude 12AM and 5PM from selection\">\n        <EuiDatePicker\n          showTimeSelect\n          selected={startDate4}\n          onChange={setStartDate4}\n          excludeTimes={[\n            moment().hours(0).minutes(0),\n            moment().hours(17).minutes(0),\n          ]}\n        />\n      </EuiFormRow>\n\n      <EuiSpacer />\n\n      <EuiFormRow label=\"Filter so only weekdays are selectable\">\n        <EuiDatePicker\n          showTimeSelect\n          selected={startDate5}\n          onChange={setStartDate5}\n          filterDate={isWeekday}\n        />\n      </EuiFormRow>\n    </div>\n  );\n};\n"]}]}
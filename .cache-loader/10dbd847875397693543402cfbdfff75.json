,GACI,sCADJ,GAEII,SAHN;AAKA,wBACE,oBAAC,WAAD;AAAa,MAAA,GAAG,EAAEH,KAAlB;AAAyB,MAAA,IAAI,EAAE;AAA/B,oBACE,oBAAC,OAAD;AACE,MAAA,KAAK,aAAMD,KAAN,0BACHnB,QAAQ,CAACqB,GAAG,CAACG,IAAL,CAAR,CAAmBC,WAAnB,EADG,CADP;AAGE,MAAA,aAAa,eACX,gDACE,yCADF,sCAEE,+BAFF,eAGE,iDAHF,0BAJJ;AAUE,MAAA,YAAY,eAAE,wDAVhB;AAWE,MAAA,UAAU,EAAEN;AAXd,OAYG,UAACO,IAAD;AAAA,0BACC;AACE,QAAA,SAAS,EAAC,iBADZ;AAEE,QAAA,OAAO,EAAE,iBAACC,CAAD,EAAO;AACdA,UAAAA,CAAC,CAACC,QAAF,GAAa3B,gBAAgB,YAAKkB,KAAL,EAA7B,GAA6CO,IAAI,EAAjD;AACD;AAJH,sBAKE,oBAAC,OAAD;AACE,QAAA,SAAS,EAAEJ,SADb;AAEE,QAAA,IAAI,EAAC,KAFP;AAGE,QAAA,IAAI,EAAC,YAHP;AAIE,QAAA,KAAK,EAAEtB,QAAQ,CAACqB,GAAG,CAACG,IAAL;AAJjB,QALF,eAWE,oBAAC,mBAAD,qBACE,kCAAOL,KAAP,CADF,CAXF,CADD;AAAA,KAZH,CADF,CADF;AAkCD;;AAED,sBACE,oBAAC,QAAD;AAAU,IAAA,WAAW,EAAC,GAAtB;AAA0B,IAAA,KAAK,EAAC;AAAhC,kBACE,oBAAC,YAAD;AAAc,IAAA,UAAU,EAAC,GAAzB;AAA6B,IAAA,IAAI,MAAjC;AAAkC,IAAA,UAAU,EAAE;AAA9C,KACGH,MAAM,CAACa,GAAP,CAAW,UAAUV,KAAV,EAAiBC,KAAjB,EAAwB;AAClC,WAAOF,kBAAkB,CAACD,OAAD,EAAUE,KAAV,EAAiBC,KAAjB,CAAzB;AACD,GAFA,CADH,CADF,CADF;AASD,CAvDM","sourcesContent":["import React from 'react';\nimport { getSassVars } from '../_get_sass_vars';\n\nimport {\n  EuiFlexGroup,\n  EuiFlexItem,\n  EuiIcon,\n  EuiCopy,\n  EuiScreenReaderOnly,\n  EuiPanel,\n} from '../../../../../src/components';\nimport { rgbToHex } from '../../../../../src/services';\n\nexport function scrollToSelector(selector, attempts = 5) {\n  const element = document.querySelector(selector);\n\n  if (element) {\n    window.scrollTo({ top: element.offsetTop - 168, behavior: 'smooth' }); // Offset affords for the sticky contrast slider\n  } else if (attempts > 0) {\n    setTimeout(scrollToSelector.bind(null, selector, attempts - 1), 250);\n  }\n}\n\nexport const CorePalette = ({ theme, colors }) => {\n  const palette = getSassVars(theme);\n\n  function renderPaletteColor(palette, color, index) {\n    const hex = palette[color];\n    const iconClass =\n      color === 'euiColorLightestShade' || color === 'euiColorEmptyShade'\n        ? 'colorGuidelines_colorPreviewTooLight'\n        : undefined;\n\n    return (\n      <EuiFlexItem key={index} grow={false}>\n        <EuiCopy\n          title={`$${color}:\n          ${rgbToHex(hex.rgba).toUpperCase()}`}\n          beforeMessage={\n            <small>\n              <kbd>Click</kbd> to copy color name\n              <br />\n              <kbd>Shift + Click</kbd> to scroll to section\n            </small>\n          }\n          afterMessage={<small>Color name copied!</small>}\n          textToCopy={color}>\n          {(copy) => (\n            <button\n              className=\"eui-isFocusable\"\n              onClick={(e) => {\n                e.shiftKey ? scrollToSelector(`#${color}`) : copy();\n              }}>\n              <EuiIcon\n                className={iconClass}\n                size=\"xxl\"\n                type=\"stopFilled\"\n                color={rgbToHex(hex.rgba)}\n              />\n              <EuiScreenReaderOnly>\n                <span>{color}</span>\n              </EuiScreenReaderOnly>\n            </button>\n          )}\n        </EuiCopy>\n      </EuiFlexItem>\n    );\n  }\n\n  return (\n    <EuiPanel paddingSize=\"l\" color=\"subdued\">\n      <EuiFlexGroup gutterSize=\"s\" wrap responsive={false}>\n        {colors.map(function (color, index) {\n          return renderPaletteColor(palette, color, index);\n        })}\n      </EuiFlexGroup>\n    </EuiPanel>\n  );\n};\n"]}]}
_THEME_LIGHT;\n  const euiPartitionConfig = euiChartTheme.partition;\n\n  return (\n    <div>\n      <EuiFlexGrid columns={2}>\n        <EuiFlexItem>\n          <EuiTitle className=\"eui-textCenter\" size=\"xs\">\n            <h3>Year to date PR count by status</h3>\n          </EuiTitle>\n          <EuiSpacer />\n          <Chart size={{ height: 200 }}>\n            <Partition\n              id=\"pieByPR\"\n              data={[\n                {\n                  status: 'Open',\n                  count: 25,\n                },\n                {\n                  status: 'Closed',\n                  count: 319,\n                },\n              ]}\n              valueAccessor={(d) => d.count}\n              layers={[\n                {\n                  groupByRollup: (d) => d.status,\n                  shape: {\n                    fillColor: (d) =>\n                      euiChartTheme.theme.colors.vizColors[d.sortIndex],\n                  },\n                },\n              ]}\n              config={{\n                ...euiPartitionConfig,\n                clockwiseSectors: false,\n              }}\n            />\n          </Chart>\n        </EuiFlexItem>\n        <EuiFlexItem>\n          <EuiTitle className=\"eui-textCenter\" size=\"xs\">\n            <h3>Code languages</h3>\n          </EuiTitle>\n          <EuiSpacer />\n          <Chart size={{ height: 200 }}>\n            <Partition\n              id=\"donutByLanguage\"\n              data={[\n                {\n                  language: 'JavaScript',\n                  percent: 51.4,\n                },\n                {\n                  language: 'TypeScript',\n                  percent: 39.6,\n                },\n                {\n                  language: 'CSS',\n                  percent: 8.7,\n                },\n              ]}\n              valueAccessor={(d) => Number(d.percent)}\n              valueFormatter={() => ''}\n              layers={[\n                {\n                  groupByRollup: (d) => d.language,\n                  shape: {\n                    fillColor: (d) =>\n                      euiChartTheme.theme.colors.vizColors[d.sortIndex],\n                  },\n                },\n              ]}\n              config={{\n                ...euiPartitionConfig,\n                emptySizeRatio: 0.4,\n                clockwiseSectors: false,\n              }}\n            />\n          </Chart>\n        </EuiFlexItem>\n      </EuiFlexGrid>\n    </div>\n  );\n};\n"]}]}
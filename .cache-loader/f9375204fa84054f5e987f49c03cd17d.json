AAI,EAAE,SADR;AAEEC,MAAAA,IAAI,EAAE,KAFR;AAGEC,MAAAA,OAAO,EAAE,mBAAM;AACbJ,QAAAA,YAAY;AACb;AALH,KAfK,EAsBL;AACEE,MAAAA,IAAI,EAAE,WADR;AAEEC,MAAAA,IAAI,EAAE,KAFR;AAGEC,MAAAA,OAAO,EAAE,mBAAM;AACbJ,QAAAA,YAAY;AACb;AALH,KAtBK,EA6BL;AACEE,MAAAA,IAAI,EAAE,kBADR;AAEEC,MAAAA,IAAI,EAAE,QAFR;AAGEC,MAAAA,OAAO,EAAE,mBAAM;AACbJ,QAAAA,YAAY;AACb;AALH,KA7BK,EAoCL;AACEE,MAAAA,IAAI,EAAE,mBADR;AAEEC,MAAAA,IAAI,EAAE,KAFR;AAGEC,MAAAA,OAAO,EAAE,mBAAM;AACbJ,QAAAA,YAAY;AACb;AALH,KApCK,EA2CL;AACEE,MAAAA,IAAI,EAAE,YADR;AAEEC,MAAAA,IAAI,EAAE,OAFR;AAGEC,MAAAA,OAAO,EAAE,mBAAM;AACbJ,QAAAA,YAAY;AACb;AALH,KA3CK;AAFS,GAAlB;AAuDA,sBACE,oBAAC,UAAD;AACE,IAAA,MAAM,EAAEH,aADV;AAEE,IAAA,YAAY,EAAEG,YAFhB;AAGE,IAAA,MAAM,eACJ,oBAAC,aAAD;AACE,MAAA,OAAO,EAAED,aADX;AAEE,MAAA,KAAK,EAAC,MAFR;AAGE,MAAA,QAAQ,EAAC,QAHX;AAIE,oBAAW,oBAJb;AAKE,MAAA,KAAK,EAAC;AALR,MAJJ;AAYE,IAAA,cAAc,EAAC,YAZjB;AAaE,IAAA,gBAAgB,EAAC;AAbnB,kBAcE,oBAAC,eAAD;AAAiB,IAAA,WAAW,EAAC;AAA7B,0BAdF,eAeE,oBAAC,cAAD;AAAgB,IAAA,cAAc,EAAE,CAAhC;AAAmC,IAAA,MAAM,EAAEX,gBAAgB,CAACa,SAAD;AAA3D,IAfF,CADF;AAmBD,CArFD","sourcesContent":["import React, { useState } from 'react';\n\nimport {\n  EuiButtonIcon,\n  EuiPopover,\n  EuiContextMenu,\n  EuiPopoverTitle,\n} from '../../../../src/components';\n\nfunction flattenPanelTree(tree, array = []) {\n  array.push(tree);\n\n  if (tree.items) {\n    tree.items.forEach((item) => {\n      if (item.panel) {\n        flattenPanelTree(item.panel, array);\n        item.panel = item.panel.id;\n      }\n    });\n  }\n\n  return array;\n}\n\nexport default () => {\n  const [isPopoverOpen, setPopover] = useState(false);\n\n  const togglePopover = () => {\n    setPopover(!isPopoverOpen);\n  };\n\n  const closePopover = () => {\n    setPopover(false);\n  };\n\n  const panelTree = {\n    id: 0,\n    items: [\n      {\n        name: 'Enable all',\n        icon: 'eye',\n        onClick: () => {\n          closePopover();\n        },\n      },\n      {\n        name: 'Disable all',\n        icon: 'eyeClosed',\n        onClick: () => {\n          closePopover();\n        },\n      },\n      {\n        name: 'Pin all',\n        icon: 'pin',\n        onClick: () => {\n          closePopover();\n        },\n      },\n      {\n        name: 'Unpin all',\n        icon: 'pin',\n        onClick: () => {\n          closePopover();\n        },\n      },\n      {\n        name: 'Invert inclusion',\n        icon: 'invert',\n        onClick: () => {\n          closePopover();\n        },\n      },\n      {\n        name: 'Invert visibility',\n        icon: 'eye',\n        onClick: () => {\n          closePopover();\n        },\n      },\n      {\n        name: 'Remove all',\n        icon: 'trash',\n        onClick: () => {\n          closePopover();\n        },\n      },\n    ],\n  };\n\n  return (\n    <EuiPopover\n      isOpen={isPopoverOpen}\n      closePopover={closePopover}\n      button={\n        <EuiButtonIcon\n          onClick={togglePopover}\n          color=\"text\"\n          iconType=\"filter\"\n          aria-label=\"Change all filters\"\n          title=\"Change all filters\"\n        />\n      }\n      anchorPosition=\"downCenter\"\n      panelPaddingSize=\"none\">\n      <EuiPopoverTitle paddingSize=\"s\">Change all filters</EuiPopoverTitle>\n      <EuiContextMenu initialPanelId={0} panels={flattenPanelTree(panelTree)} />\n    </EuiPopover>\n  );\n};\n"]}]}